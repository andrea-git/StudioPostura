       Screen1-Gd-1-Event-Proc.
           .

      * USER DEFINE PARAGRAPH
       CALCOLA-TOTALE.
      * <TOTEM:PARA. CALCOLA-TOTALE>
           move 0 to importo-sub.
           move 0 to importo-iva.
           move 0 to importo-tot.

           inquire form1-gd-1, last-row in tot-righe.
           perform varying store-riga from 2 by 1 
                     until store-riga > tot-righe
              inquire form1-gd-1(store-riga, 4), cell-data in como-qta
              inquire form1-gd-1(store-riga, 5), cell-data in como-prz
              if como-qta = 0
                 move 1 to como-qta
              end-if
              compute importo-sub = 
                      importo-sub + como-qta * como-prz
           end-perform.
           move save-iva to iva-codice.
           read codiva no lock.

           compute iva-3dec = importo-sub * iva-aliquota / 100
           add 0,005     to iva-3dec giving iva-2dec
           move iva-2dec to importo-iva
           compute importo-tot = 
                   importo-sub + 
                   importo-iva.

           move importo-sub to lab-sub-buf.
           move importo-iva to lab-iva-buf.
           move importo-tot to lab-tot-buf.
           display lab-sub lab-iva lab-tot 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-cliente
                inquire ef-cliente, value in cli-codice
                move "clienti-alfa" to como-file
                call "zoom-gt"   using como-file, cli-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   perform DESCRIZIONE-CLIENTE
                   display lab-cliente                        
                   move cli-codice to ef-cliente-buf
                   display ef-cliente
                end-if
           when 78-ID-form1-gd-1
                inquire form1-gd-1(riga, 1), cell-data in art-codice
                move "articoli"     to Como-File
                call   "zoom-gt" using  como-file, art-rec
                                 giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move art-codice        to col-art
                   move art-descrizione   to col-des
                   modify form1-gd-1(riga, 1), cell-data = col-art
                   modify form1-gd-1(riga, 2), cell-data = col-des
                   perform CONTROLLO-RIGA
                end-if
                
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-data
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-buf
                display ef-data
               
           when 78-ID-ef-cliente
                inquire ef-cliente, value in cli-codice
                read clienti no lock                   
                     invalid
                     set errori to true
                     display message "Cliente NON valido"
                               title tit-err
                                icon 2
                     move spaces to lab-cliente-buf
                 not invalid
                     perform DESCRIZIONE-CLIENTE
                     if cli-iva-esente not = spaces
                        move cli-iva-esente to save-iva
                     end-if
                end-read
                display lab-cliente

           end-evaluate.

           if errori
              move 4 to accept-control
           end-if 
                
           .
      * <TOTEM:END>

       CONTROLLO-RIGA.
      * <TOTEM:PARA. CONTROLLO-RIGA>
           set tutto-ok to true.  

           evaluate colonna
           when 1
                move col-art to art-codice
                if art-codice not = spaces
                   read articoli no lock
                        invalid
                        move spaces to art-descrizione
                        set errori to true
                        display message "Articolo NON valido"
                                  title tit-err
                                icon 2
                   end-read
                   move art-descrizione to col-des
                   modify form1-gd-1(riga, 2), cell-data col-des
                end-if
           when 2
                if col-art = spaces and col-des = spaces
                   set errori to true
                   display message "Articolo o Descrizione obbligatorio"
                             title tit-err
                              icon 2
                end-if

           when 3
                perform CALCOLA-TOTALE

           when 4
                if como-prz = 0 and col-art not = spaces
                   set errori to true
                   display message "Prezzo obbligatorio"
                             title tit-err
                              icon 2
                end-if
                if tutto-ok
                   perform CALCOLA-TOTALE
                end-if
      
           end-evaluate 
           .
      * <TOTEM:END>

       DESCRIZIONE-CLIENTE.
      * <TOTEM:PARA. DESCRIZIONE-CLIENTE>
           initialize lab-cliente-buf.
           inspect cli-ragsoc replacing trailing spaces by low-value.
           string  cli-ragsoc delimited low-value
                   " "        delimited size
                   cli-nome   delimited size
              into lab-cliente-buf
           end-string 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "utydata.cpy".
           copy "color-custom.cpy" 
           .
      * <TOTEM:END>

       RESETTA-GRID.
      * <TOTEM:PARA. RESETTA-GRID>
           modify form1-gd-1, reset-grid = 1.
           perform FORM1-GD-1-CONTENT.

           modify gd-note, reset-grid = 1.
           perform GD-NOTE-CONTENT 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE.
      * <TOTEM:PARA. RIEMPI-CHIAVE>
           move tno-data-doc(1:4) to tno-anno.
           move como-numero       to tno-numero 
           .
      * <TOTEM:END>

       SPOSTAMENTO.
      * <TOTEM:PARA. SPOSTAMENTO>
           set tutto-ok to true.
           inquire form1-gd-1, cursor-y in riga,
                               cursor-x in colonna,
                               last-row in tot-righe.
      
           if event-data-2 not = riga
      
              perform VALORE-RIGA

              if not RigaVuota
                 perform varying colonna from 1 by 1
                           until colonna > 5
                    perform CONTROLLO-RIGA
                    if errori exit perform end-if
                 end-perform
              end-if

              if tutto-ok
                 evaluate true
                 when event-data-2 < 2 
                      move 2            to riga
                      move event-data-1 to colonna
      
                 when event-data-2 > tot-righe
                      add  1                to tot-righe giving riga
                      set event-action      to event-action-fail
                      move 1                to colonna
                      perform PUNTA-CELLA
                 when other
                      move event-data-2 to riga
                      move event-data-1 to colonna
                 end-evaluate
                 perform VALORE-RIGA
                 if art-codice = 0
                    initialize rno-rec 
                               replacing numeric data by zeroes
                                    alphanumeric data by spaces
                 end-if
                 move rno-rec       to old-rno-rec
              else                  
                 set event-action   to event-action-fail
                 perform PUNTA-CELLA
              end-if
           else
              if event-data-1 not = colonna
                 inquire Form1-Gd-1(riga, 1), cell-data in art-codice
                 if art-codice = 0
                    set event-action to event-action-fail
                    move 1 to colonna event-data-1
                    perform PUNTA-CELLA
                 end-if
              end-if
              move event-data-1 to colonna
           end-if.
      
           perform COLORE-RIGA.
           move BitmapZoomDisabled to BitmapNumZoom.
           move 0 to e-cerca.

           if colonna = 1
              move BitmapZoomEnabled to BitmapNumZoom
              move 1 to e-cerca
           end-if.
           
           modify tool-cerca, enabled e-cerca, 
                              bitmap-number BitmapNumZoom.


      ***---
       COLORE-RIGA.
           modify  form1-gd-1, start-y = event-data-2, 
                                     y = event-data-2,
                               start-x = 1, 
                                     x = 5,
                          region-color = 353.


      ***---
       PUNTA-CELLA.
           move 78-ID-Form1-Gd-1 to control-id store-id.
           move 4                to accept-control.
           modify Form1-Gd-1,    cursor-y riga, 
                                 cursor-x colonna.
           move riga to event-data-2.
           perform COLORE-RIGA 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           set SiSalvato to true.
           perform Form1-BUF-TO-FLD.
           move 0 to scelta.

           display message "Abbandonare la creazione del documento?"
                     title titolo
                      type mb-yes-no
                    giving scelta       
      
           evaluate scelta
           when mb-yes 
                continue
           when mb-no
                perform CANCELLA-COLORE
                set errori to true
                move store-id to CONTROL-ID       
                move 4        to ACCEPT-CONTROL   
           end-evaluate 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           set tutto-ok to true.
           perform varying control-id from 78-ID-ef-data by 1 
                     until control-id > 78-ID-ef-cliente
              perform CONTROLLO
              if errori
                 exit perform
              end-if
           end-perform.

           if tutto-ok
              inquire ef-data, value in ef-data-buf
              move ef-data-buf to como-data
              perform DATE-TO-FILE
              move como-data(1:4) to como-anno
              move como-anno   to tno-anno
              move como-numero to tno-numero
              read tnotacr no lock
                   invalid continue
               not invalid
                   set errori to true
                   move tno-data-doc to como-data
                   perform DATE-TO-SCREEN
                   move como-data to data-doc
                   display message "ESISTE GIA' LA NOTA CREDITO N. " 
           tno-numero
                            x"0d0a""CON DATA " data-doc
                            x"0d0a""CONTROLLARE I CONTATORI NEI PARAMETR
      -    "I DITTA"
                             title tit-err
                              icon 2
              end-read
           end-if.
      
           if tutto-ok                        
              move low-value to tno-rec
              move como-data to tno-data-doc
              start tnotacr key >= tno-k-data
                    invalid continue
                not invalid
                    perform until 1 = 2
                       read tnotacr next at end exit perform end-read
                       if tno-anno not = como-anno
                          exit perform 
                       end-if
                       if tno-numero < como-numero
                          if tno-data-doc > como-data
                             set errori to true
                          end-if
                       end-if
                       if tno-numero > como-numero
                          if tno-data-doc < como-data
                             set errori to true
                          end-if
                       end-if
                       if errori
                          move 78-ID-ef-data to store-id control-id
                          move tno-data-doc to como-data
                          perform DATE-TO-SCREEN
                          move como-data to data-doc
                          display message "ESISTE NOTA CREDITO N. " 
           tno-numero
                                   x"0d0a""CON DATA " data-doc
                                    title tit-err
                                     icon 2
                          exit perform
                       end-if
                    end-perform
              end-start
           end-if.

           if tutto-ok
              if importo-sub = 0
                 set errori to true
                 move 78-ID-form1-gd-1 to control-id
                 display message "IMPOSSIBILE REGISTRARE DOCUMENTO A ZER
      -    "O"
                            title tit-err
                             icon 2
              end-if
           end-if.

           if tutto-ok
              move 78-ID-form1-gd-1 to control-id
              inquire form1-gd-1, cursor-y in riga
              perform VALORE-RIGA
              perform varying colonna from 1 by 1 
                        until colonna > 4
                 perform CONTROLLO-RIGA
                 if errori
                    exit perform
                 end-if
              end-perform
           end-if.
      
           if errori
              perform CANCELLA-COLORE
              move 4 to ACCEPT-CONTROL
           else
              perform FORM1-BUF-TO-FLD
              perform CANCELLA-COLORE 

              close    param
              open i-o param
              move spaces to prm-chiave
              read param  no lock
              move como-numero to prm-notecr-num
              if tno-data-doc > prm-notecr-data
                 move tno-data-doc to prm-notecr-data
              end-if
              rewrite prm-rec
              close      param 
              open input param

              move importo-sub to tno-importo-sub
              move importo-iva to tno-importo-iva
              move importo-tot to tno-importo-tot

              write tno-rec       

              inquire form1-gd-1, last-row in tot-righe
              perform varying riga from 2 by 1 
                        until riga > tot-righe
                 move tno-chiave to rno-chiave-testa
                 perform VALORE-RIGA
                 subtract 1 from riga giving rno-riga
                 move col-art     to rno-articolo
                 move col-des     to rno-descrizione
                 move col-listino to rno-listino
                 move col-qta     to rno-qta
                 move col-prz     to rno-prz
                 move tno-iva     to rno-iva
                 write rno-rec
              end-perform

              display message "CREATA NOTA CREDITO N. " prm-notecr-num
                        title titolo

              move spaces to link-stampante

              call   "st-docum" using tno-chiave, "N", link-stampante
              cancel "st-docum"

              set vecchio to true       
              move 27 to key-status
           end-if.   
                 
           display form1 
           .
      * <TOTEM:END>

       VALORE-RIGA.
      * <TOTEM:PARA. VALORE-RIGA>
           inquire form1-gd-1(riga, 1), cell-data in col-art.
           inquire form1-gd-1(riga, 2), cell-data in col-des.
           inquire form1-gd-1(riga, 3), cell-data in col-listino.
           inquire form1-gd-1(riga, 4), cell-data in col-qta.
           inquire form1-gd-1(riga, 5), cell-data in col-prz.

           move col-qta to como-qta.
           move col-prz to como-prz.

           if col-art      = spaces and
              col-des      = spaces and
              como-qta     = 0 and
              como-prz     = 0
              set RigaVuota to true
           else
              set RigaVuota to false
           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-OLD.
      * <TOTEM:PARA. VALORIZZA-OLD>
           set vecchio to true 
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       TOOL-SALVA-LinkTo.
      * <TOTEM:PARA. TOOL-SALVA-LinkTo>
           perform SALVA 
           .
      * <TOTEM:END>
       TOOL-CERCA-LinkTo.
      * <TOTEM:PARA. TOOL-CERCA-LinkTo>
           inquire tool-cerca, enabled in e-cerca.
           if e-cerca = 1
              perform CERCA
           end-if 
           .
      * <TOTEM:END>
       cbo-pagamento-BeforeProcedure.
      * <TOTEM:PARA. cbo-pagamento-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       cbo-pagamento-AfterProcedure.
      * <TOTEM:PARA. cbo-pagamento-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       cbo-iban-BeforeProcedure.
      * <TOTEM:PARA. cbo-iban-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       cbo-iban-AfterProcedure.
      * <TOTEM:PARA. cbo-iban-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       ef-data-BeforeProcedure.
      * <TOTEM:PARA. ef-data-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-cliente-BeforeProcedure.
      * <TOTEM:PARA. ef-cliente-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           move BitmapZoomEnabled to BitmapNumZoom.
           move 1 to e-cerca.
           modify tool-cerca, enabled e-cerca, 
                              bitmap-number BitmapNumZoom 
           .
      * <TOTEM:END>
       ef-numero-BeforeProcedure.
      * <TOTEM:PARA. ef-numero-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-anno-BeforeProcedure.
      * <TOTEM:PARA. ef-anno-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-data-AfterProcedure.
      * <TOTEM:PARA. ef-data-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           perform CONTROLLO 
           .
      * <TOTEM:END>
       ef-cliente-AfterProcedure.
      * <TOTEM:PARA. ef-cliente-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           perform CONTROLLO.
           if tutto-ok
              move BitmapZoomEnabled to BitmapNumZoom
              move 0 to e-cerca
              modify tool-cerca, enabled e-cerca, 
                                 bitmap-number BitmapNumZoom
           end-if 
           .
      * <TOTEM:END>
       ef-numero-AfterProcedure.
      * <TOTEM:PARA. ef-numero-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       ef-anno-AfterProcedure.
      * <TOTEM:PARA. ef-anno-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       Form1-Gd-1-Ev-Msg-Begin-Drag.
      * <TOTEM:PARA. Form1-Gd-1-Ev-Msg-Begin-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       Form1-Gd-1-Ev-Msg-End-Drag.
      * <TOTEM:PARA. Form1-Gd-1-Ev-Msg-End-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       Form1-Gd-1-Ev-Msg-Goto-Cell.
      * <TOTEM:PARA. Form1-Gd-1-Ev-Msg-Goto-Cell>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       Form1-Gd-1-Ev-Msg-Goto-Cell-Drag.
      * <TOTEM:PARA. Form1-Gd-1-Ev-Msg-Goto-Cell-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       Form1-Gd-1-Ev-Msg-Goto-Cell-Mouse.
      * <TOTEM:PARA. Form1-Gd-1-Ev-Msg-Goto-Cell-Mouse>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       Form1-Gd-1-Ev-Msg-Finish-Entry.
      * <TOTEM:PARA. Form1-Gd-1-Ev-Msg-Finish-Entry>
           set tutto-ok to true.
           inquire form1-gd-1, cursor-y in riga, last-row in tot-righe.
           perform VALORE-RIGA.
      
           evaluate colonna
           when 1
                if col-art = spaces
                   if not RigaVuota
                      display message "Cancellare la riga?"
                                title titolo
                                 type mb-yes-no
                              default mb-no
                               giving scelta
                      if scelta = mb-no
                         set errori to true
                      else
                         initialize rec-grid 
                                    replacing numeric data by zeroes
                                         alphanumeric data by spaces
      
                         move riga to store-riga
                         add 1 to riga
                         perform VALORE-RIGA
                         move rno-rec    to old-rno-rec
                         move store-riga to riga
                 
                         modify form1-gd-1, record-to-delete = riga
                         modify form1-gd-1, insert-rows = 1
                      end-if
                   end-if
                else
                   perform CONTROLLO-RIGA
                end-if
                if tutto-ok
                   if rno-articolo not = old-rno-articolo
                      move riga to RigaCambiata
                      move 1    to ColonnaCambiata
                   end-if
                end-if
      
           when 2
                perform CONTROLLO-RIGA
                if tutto-ok
                   if col-des not = old-col-des
                      move riga to RigaCambiata
                      move 2    to ColonnaCambiata
                   end-if
                end-if
      
           when 4
                perform CONTROLLO-RIGA
                if col-qta not = old-col-qta
                   move riga  to RigaCambiata
                   move 3     to ColonnaCambiata
                end-if

           when 5
                perform CONTROLLO-RIGA
                if col-prz not = old-col-prz
                   move riga  to RigaCambiata
                   move 4     to ColonnaCambiata
                end-if 
           end-evaluate.
      
           if errori
              move 78-ID-form1-gd-1 to store-id
              set event-action to event-action-fail
           else                                              
              modify form1-gd-1(riga, 1),   cell-data col-art
              modify form1-gd-1(riga, 2),   cell-data col-des
              modify form1-gd-1(riga, 3),   cell-data col-listino
              modify form1-gd-1(riga, 4),   cell-data col-qta
              modify form1-gd-1(riga, 5),   cell-data col-prz
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if 
           .
      * <TOTEM:END>
       Form1-Gd-1-Ev-Msg-Begin-Entry.
      * <TOTEM:PARA. Form1-Gd-1-Ev-Msg-Begin-Entry>
           evaluate colonna 
           when 3
                set event-action to event-action-fail
           when 5
                set environment "KEYSTROKE" to "DATA=44 46"  
           end-evaluate 
           .
      * <TOTEM:END>

      *{TOTEM}END

      *{TOTEM}SHOW-MSG
       MESSAGE-FOR-FILEERROR.
           CALL "C$RERRNAME" USING TOTEM-MSG-ERR-FILE
           CALL "C$RERR" USING EXTEND-STAT, TEXT-MESSAGE
              MOVE PRIMARY-ERROR TO TOTEM-MSG-ID
           .

       SHOW-TRANSACTION-ROUTINE.
           IF TOTEM-TRANSACTION-FLAG = SPACES
              IF TRANSACTION-STATUS NOT = 0
                 STRING "TRANSACTION ERROR ", TRANSACTION-STATUS
                    DELIMITED BY SIZE INTO TOTEM-MSG-1
              END-IF
           ELSE
              PERFORM SHOW-MSG-ROUTINE
              IF TRANSACTION-STATUS = 0
                 MOVE "Transazione terminata con Rollback." TO 
           TOTEM-MSG-3
              ELSE
                 STRING "TRANSACTION ERROR ", TRANSACTION-STATUS
                    DELIMITED BY SIZE INTO TOTEM-MSG-3
              END-IF
           END-IF
           .

       SHOW-MSG-ROUTINE.
           MOVE SPACE TO TOTEM-MSG-1 TOTEM-MSG-2 TOTEM-MSG-3
           EVALUATE TOTEM-MSG-ID
               WHEN "10"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Nessun altro dato." TO TOTEM-MSG-2
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "22"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Chiave Duplicata." TO TOTEM-MSG-2
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "23"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Raggiunta fine file." TO TOTEM-MSG-2
                    MOVE MB-WARNING-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "101"
                    MOVE "Terminare l'applicazione?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "201"
                    MOVE "Aggiungere un nuovo record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "202"
                    MOVE "Aggiornare il Record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "203"
                    MOVE "Cancellare il Record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "204"
                    MOVE "Chiave duplicata." TO TOTEM-MSG-1
                    MOVE MB-WARNING-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "301"
                    MOVE "Inserimento avvenuto con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "302"
                    MOVE "Aggiornamento avvenuto con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "303"
                    MOVE "Cancellazione avvenuta con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "401"
                    MOVE "Shell non trovata." TO TOTEM-MSG-1
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN OTHER
                    MOVE TEXT-MESSAGE TO TOTEM-MSG-1
                    STRING "FILE:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-3
                    MOVE 0 TO TOTEM-IDX1
                    INSPECT TOTEM-MSG-3 TALLYING TOTEM-IDX1
                       FOR TRAILING SPACE
                    STRING TOTEM-MSG-3(1:TOTEM-MSG-LENGTH - 
           TOTEM-IDX1), 
                       ", FILE STATUS ", PRIMARY-ERROR "," 
                       SECONDARY-ERROR
                    DELIMITED BY SIZE INTO TOTEM-MSG-2
                    MOVE SPACES TO TOTEM-MSG-3
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
           END-EVALUATE
           PERFORM MESSAGE-BOX-ROUTINE
           .

       MESSAGE-BOX-ROUTINE.
           MOVE 1 TO TOTEM-MSG-TEXT-POINTER
           IF TOTEM-MSG-1 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-1 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              STRING TOTEM-MSG-1( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                 DELIMITED BY SIZE
                 INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-2 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-2 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              IF TOTEM-MSG-TEXT-POINTER > 1
                 STRING X"0A" DELIMITED BY SIZE
                     INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
              END-IF
              STRING TOTEM-MSG-2( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                  DELIMITED BY SIZE
                  INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-3 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-3 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              IF TOTEM-MSG-TEXT-POINTER > 1
                 STRING X"0A" DELIMITED BY SIZE
                     INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
              END-IF
              STRING TOTEM-MSG-3( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                  DELIMITED BY SIZE
                  INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-TEXT-POINTER = 1
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-TEXT TALLYING TOTEM-MSG-SIZE FOR 
           TRAILING SPACE
                  COMPUTE TOTEM-MSG-TEXT-POINTER = 
           TOTEM-MSG-FULL-LENGTH - TOTEM-MSG-SIZE + 1
           END-IF
           MOVE LOW-VALUES TO TOTEM-MSG-TEXT(TOTEM-MSG-TEXT-POINTER : 1 
           )
           .

       MESSAGE-BOX-DISPLAY.
           PERFORM SHOW-MSG-ROUTINE
           PERFORM MESSAGE-BOX-ROUTINE
           DISPLAY MESSAGE BOX TOTEM-MSG-TEXT
               TITLE IS TOTEM-MSG-TITLE
               TYPE  IS TOTEM-MSG-BUTTON-TYPE
               ICON  IS TOTEM-MSG-DEFAULT-BUTTON
               RETURNING TOTEM-MSG-RETURN-VALUE
           .

      *{TOTEM}END

