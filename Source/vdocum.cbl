      * USER DEFINE PARAGRAPH
       ABILITAZIONI.
      * <TOTEM:PARA. ABILITAZIONI>
           if mod = 1
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move 0 to e-salva e-cancella
           end-if.

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-salva, tool-cancella, tool-modifica.
      * FINE 
           .
      * <TOTEM:END>

       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = 0
              exit paragraph 
           end-if.
      *
           display message box MSG-Cancellare-il-record-corrente
                   title   titolo
                   type    mb-yes-no 
                   default mb-no
                   giving  scelta

           if scelta = mb-yes
              move low-value to rsc-rec
              perform RIEMPI-CHIAVE
              evaluate true
              when vdocum-tipo-fattura
                   delete tfatture record invalid continue end-delete

                   move low-value  to rfa-rec
                   move tfa-chiave to rfa-chiave-testa
                   start rfatture key >= rfa-chiave
                         invalid continue
                     not invalid
                         perform until 1 = 2
                            read rfatture next at end exit perform 
           end-read
                            if rfa-chiave-testa not = tfa-chiave
                               exit perform
                            end-if
                            delete rfatture record
                         end-perform
                   end-start                           
                   perform CANCELLA-RIFERIMENTO-FATTURA-SU-SCHEDA
              when vdocum-tipo-corrisp
                   delete tcorrisp record invalid continue end-delete

                   move low-value  to rco-rec
                   move tco-chiave to rco-chiave-testa
                   start rcorrisp key >= rco-chiave
                         invalid continue
                     not invalid
                         perform until 1 = 2
                            read rcorrisp next at end exit perform 
           end-read
                            if rco-chiave-testa not = tco-chiave
                               exit perform
                            end-if
                            delete rcorrisp record
                         end-perform
                   end-start
                   perform CANCELLA-RIFERIMENTO-CORRISP-SU-SCHEDA
              when vdocum-tipo-nota
                   delete tnotacr record invalid continue end-delete   

                   move low-value  to rno-rec
                   move tno-chiave to rno-chiave-testa
                   start rnotacr key >= rno-chiave
                         invalid continue
                     not invalid
                         perform until 1 = 2
                            read rnotacr next at end exit perform 
           end-read
                            if rno-chiave-testa not = tno-chiave
                               exit perform
                            end-if
                            delete rnotacr record
                         end-perform
                   end-start
              end-evaluate 

              move 0    to mod
              move 1    to mod-k
              perform FORM1-CLEAR
              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC
              
              display message box 
           MSG-Cancellazione-avvenuta-con-successo
                      title titolo                         

              unlock tfatture all records
              unlock tcorrisp all records
              unlock tnotacr  all records

              move 27 to key-status
           end-if 
           .
      * <TOTEM:END>

       CANCELLA-CAMBIA-RIFERIMENTI.
      * <TOTEM:PARA. CANCELLA-CAMBIA-RIFERIMENTI>
      ***---
       CANCELLA-RIFERIMENTO-FATTURA-SU-SCHEDA.
           move tfa-numero   to rsc-numfat.
           move tfa-data-doc to rsc-datafat.
           start rschede key >= rsc-k-fatcli
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read rschede next at end exit perform end-read
                    if rsc-numfat  not = tfa-numero or
                       rsc-datafat not = tfa-data-doc
                       exit perform
                    end-if
                    if rsc-cliente = old-tfa-cliente
                       move 0 to rsc-numfat
                       move 0 to rsc-datafat
                       rewrite rsc-rec
                    end-if
                 end-perform
           end-start.

      ***---
       CANCELLA-RIFERIMENTO-CORRISP-SU-SCHEDA.
           move tco-numero   to rsc-numcor.
           move tco-data-doc to rsc-datacor.
           start rschede key >= rsc-k-corcli
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read rschede next at end exit perform end-read
                    if rsc-numcor  not = tco-numero or
                       rsc-datacor not = tco-data-doc
                       exit perform
                    end-if
                    if rsc-cliente = old-tco-cliente
                       move 0 to rsc-numcor
                       move 0 to rsc-datacor
                       rewrite rsc-rec
                    end-if
                 end-perform
           end-start.

      ***---
       CAMBIA-RIF-DATA-FATTURA-SU-SCHEDA.
           move tfa-numero       to rsc-numfat.
           move old-tfa-data-doc to rsc-datafat.
           start rschede key >= rsc-k-fatcli
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read rschede next at end exit perform end-read
                    if rsc-numfat  not = tfa-numero or
                       rsc-datafat not = old-tfa-data-doc
                       exit perform
                    end-if
                    if rsc-cliente = old-tfa-cliente
                       move tfa-data-doc to rsc-datafat
                       rewrite rsc-rec
                    end-if
                 end-perform
           end-start.

      ***---
       CAMBIA-RIF-DATA-CORRISP-SU-SCHEDA.
           move tco-numero       to rsc-numcor.
           move old-tco-data-doc to rsc-datacor.
           start rschede key >= rsc-k-corcli
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read rschede next at end exit perform end-read
                    if rsc-numcor  not = tco-numero or
                       rsc-datacor not = old-tco-data-doc
                       exit perform
                    end-if
                    if rsc-cliente = old-tco-cliente
                       move tco-data-doc to rsc-datacor
                       rewrite rsc-rec
                    end-if
                 end-perform
           end-start 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-cliente
                inquire ef-cliente, value in cli-codice
                move "clienti-alfa" to como-file
                call "zoom-gt"   using como-file, cli-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   perform DESCRIZIONE-CLIENTE
                   display lab-cliente                        
                   move cli-codice to ef-cliente-buf
                   display ef-cliente
                end-if
           end-evaluate 
           .
      * <TOTEM:END>

       CLEAR-SCREEN.
      * <TOTEM:PARA. CLEAR-SCREEN>
           move tfa-chiave to old-tfa-chiave.

           initialize tfa-dati 
                      tco-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                         
           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.
                                          
           display form1. 

           perform RESETTA-GRID.
  
           unlock tfatture all records.
           unlock tcorrisp all records 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-data
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-buf
                display ef-data
               
           when 78-ID-ef-cliente
                inquire ef-cliente, value in cli-codice
                read clienti no lock                   
                     invalid
                     set errori to true
                     display message "Cliente NON valido"
                               title tit-err
                                icon 2
                     move spaces to lab-cliente-buf
                 not invalid
                     perform DESCRIZIONE-CLIENTE
                     if cli-iva-esente not = spaces
                        move cli-iva-esente to iva-codice
                        read codiva no lock
                        move iva-aliquota to new-aliquota
                     else
                        move prm-iva to iva-codice
                        read codiva  no lock
                        move iva-aliquota to new-aliquota
                     end-if
                end-read
                display lab-cliente

           end-evaluate.

           if errori
              move 4 to accept-control
           end-if 
                
           .
      * <TOTEM:END>

       CURRENT-RECORD.
      * <TOTEM:PARA. CURRENT-RECORD>
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if nuovo move 0 to mod end-if.

           if mod = 1
              evaluate true
              when vdocum-tipo-fattura
                   read tfatture lock invalid 
                        set errori to true 
                   end-read
              when vdocum-tipo-corrisp
                   read tcorrisp lock invalid 
                        set errori to true 
                   end-read
              when vdocum-tipo-nota
                   continue
              end-evaluate
           else
              evaluate true
              when vdocum-tipo-fattura
                   read tfatture no lock invalid 
                        set errori to true 
                   end-read
              when vdocum-tipo-corrisp
                   read tcorrisp no lock invalid 
                        set errori to true 
                   end-read
              when vdocum-tipo-nota
                   continue
              end-evaluate
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0      to mod
                    move 1      to mod-k
                    move 78-ID-ef-data to CONTROL-ID
                    move 4             to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-FLD-TO-BUF
                 display form1
                 set vecchio to true
      *****           if mod = 1
      *****              set StatusModifica to true
      *****           else                         
      *****              set StatusVisua    to true
      *****           end-if
      *****           perform STATUS-BAR-MSG
                 perform VALORIZZA-OLD
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-data to CONTROL-ID
                       move 4             to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                    end-if
                 end-if
              end-if
           end-if 
           .
      * <TOTEM:END>

       DESCRIZIONE-CLIENTE.
      * <TOTEM:PARA. DESCRIZIONE-CLIENTE>
           initialize lab-cliente-buf.
           inspect cli-ragsoc replacing trailing spaces by low-value.
           string  cli-ragsoc delimited low-value
                   " "        delimited size
                   cli-nome   delimited size
              into lab-cliente-buf
           end-string 
           .
      * <TOTEM:END>

       INIT-OLD-REC.
      * <TOTEM:PARA. INIT-OLD-REC>
           initialize old-tfa-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.

           initialize old-tco-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.

           initialize old-tno-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces 
           .
      * <TOTEM:END>

       MODIFICA.
      * <TOTEM:PARA. MODIFICA>
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1   to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    move 0   to mod-k
                    move 1   to mod
      *****              set StatusModifica to true
      *****              perform STATUS-BAR-MSG
                 end-if
              else
                 move 1 to mod
                 perform SALV-MOD            
                 move 0 to mod
                 move 1 to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    move 78-ID-ef-data to control-id
                    set NoMessage to true
                    perform CURRENT-RECORD
      *****              set StatusVisua to true
      *****              perform STATUS-BAR-MSG 
                    if vdocum-tipo-fattura
                       unlock tfatture all records
                    else
                       unlock tcorrisp all records
                    end-if
                 end-if
              end-if

              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if.

           if mod = 0
              move BitmapZoomEnabled to BitmapNumZoom
              move 0 to e-cerca
              modify tool-cerca, enabled e-cerca, 
                                 bitmap-number BitmapNumZoom
           else
              if tutto-ok
                 move 78-ID-ef-data to control-id
                 move 4 to accept-control
              end-if
           end-if 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "utydata.cpy".
           copy "color-custom.cpy" 
           .
      * <TOTEM:END>

       RESETTA-GRID.
      * <TOTEM:PARA. RESETTA-GRID>
           modify form1-gd-1, reset-grid = 1.
           perform FORM1-GD-1-CONTENT.

           modify gd-note, reset-grid = 1.
           perform GD-NOTE-CONTENT 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE.
      * <TOTEM:PARA. RIEMPI-CHIAVE>
           evaluate true
           when vdocum-tipo-fattura
                inquire ef-anno,   value in tfa-anno
                inquire ef-numero, value in tfa-numero
           when vdocum-tipo-corrisp
                inquire ef-anno,   value in tco-anno
                inquire ef-numero, value in tco-numero
           when vdocum-tipo-nota
                inquire ef-anno,   value in tno-anno
                inquire ef-numero, value in tno-numero
           end-evaluate 
           .
      * <TOTEM:END>

       SPOSTAMENTO.
      * <TOTEM:PARA. SPOSTAMENTO>
           inquire form1-gd-1, last-row in tot-righe.
           if event-data-2 < 2
              move 2 to event-data-2
           end-if.
           if event-data-2 > tot-righe
              move tot-righe to event-data-2
           end-if.
           modify  form1-gd-1, start-y = event-data-2, 
                                     y = event-data-2,
                               start-x = 1, 
                                     x = 6,
                          region-color = 353 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           set SiSalvato to true.
           if mod = 0 exit paragraph end-if.
           perform Form1-BUF-TO-FLD.
           move 0 to scelta.
           evaluate true
           when vdocum-tipo-fattura
                if tfa-data-doc not = old-tfa-data-doc
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-data to store-id 
                end-if

                if tfa-cliente not = old-tfa-cliente
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-cliente to store-id 
                end-if

                if tfa-pagamento not = old-tfa-pagamento
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-pagamento to store-id 
                end-if

                if tfa-iban not = old-tfa-iban
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-iban to store-id 
                end-if

           when vdocum-tipo-corrisp
                if tco-data-doc not = old-tco-data-doc
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-data to store-id 
                end-if

                if tco-cliente not = old-tco-cliente
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-cliente to store-id 
                end-if

                if tco-pagamento not = old-tco-pagamento
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-pagamento to store-id 
                end-if

                if tco-iban not = old-tco-iban
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-iban to store-id 
                end-if

           when vdocum-tipo-nota  
                if tno-data-doc not = old-tno-data-doc
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-data to store-id 
                end-if

                if tno-cliente not = old-tno-cliente
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-cliente to store-id 
                end-if

                if tno-pagamento not = old-tno-pagamento
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-pagamento to store-id 
                end-if

                if tno-iban not = old-tno-iban
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-iban to store-id 
                end-if
           end-evaluate.

           if NoSalvato
              display message MSG-Salvare-le-modifiche
                        title titolo
                         type mb-yes-no-cancel 
                       giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           set tutto-ok to true.
           perform varying control-id from 78-ID-ef-data by 1 
                     until control-id > 78-ID-ef-cliente
              perform CONTROLLO
              if errori
                 exit perform
              end-if
           end-perform.

           if tutto-ok
              inquire ef-data, value in ef-data-buf
              move ef-data-buf to como-data
              perform DATE-TO-FILE
              move como-data(1:4) to como-anno
              set tutto-ok to true
              evaluate true
              when vdocum-tipo-fattura
                   move ef-numero-buf to como-numero
                   move low-value to tfa-rec
                   move como-data to tfa-data-doc
                   start tfatture key >= tfa-k-data
                         invalid  
                         start tfatture key <= tfa-k-data
                               invalid  continue
                           not invalid
                               perform until 1 = 2
                                 read tfatture previous at end exit 
           perform end-read
                                 if tfa-anno not = como-anno
                                    exit perform 
                                 end-if
                                 if tfa-numero < como-numero
                                    if tfa-data-doc > como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                                 if tfa-numero > como-numero
                                    if tfa-data-doc < como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                              end-perform
                         end-start
                     not invalid
                         perform until 1 = 2
                            read tfatture next at end exit perform 
           end-read
                            if tfa-anno not = como-anno
                               exit perform 
                            end-if
                            if tfa-numero < como-numero
                               if tfa-data-doc > como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                            if tfa-numero > como-numero
                               if tfa-data-doc < como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                         end-perform
                   end-start
                   if errori
                      move 78-ID-ef-data to store-id
                      move tfa-data-doc to como-data
                      perform DATE-TO-SCREEN
                      move como-data to data-doc
                      display message "ESISTE FATTURA N. " tfa-numero
                               x"0d0a""CON DATA " data-doc
                                title tit-err
                                 icon 2
                   end-if
              when vdocum-tipo-corrisp
                   move ef-numero-buf to como-numero
                   move low-value to tco-rec
                   move como-data to tco-data-doc
                   start tcorrisp key >= tco-k-data
                         invalid   
                         start tcorrisp key <= tco-k-data
                               invalid  continue
                           not invalid
                               perform until 1 = 2
                                 read tcorrisp previous at end exit 
           perform end-read
                                 if tco-anno not = como-anno
                                    exit perform 
                                 end-if
                                 if tco-numero < como-numero
                                    if tco-data-doc > como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                                 if tco-numero > como-numero
                                    if tco-data-doc < como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                              end-perform
                         end-start
                     not invalid
                         perform until 1 = 2
                            read tcorrisp next at end exit perform 
           end-read
                            if tco-anno not = como-anno
                               exit perform 
                            end-if
                            if tco-numero < como-numero
                               if tco-data-doc > como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                            if tco-numero > como-numero
                               if tco-data-doc < como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                         end-perform
                   end-start
                   if errori
                      move 78-ID-ef-data to store-id
                      set errori to true
                      move tco-data-doc to como-data
                      perform DATE-TO-SCREEN
                      move como-data to data-doc
                      display message "ESISTE CORRISPETTIVO N. " 
           tco-numero
                               x"0d0a""CON DATA " data-doc
                                title tit-err
                                 icon 2
                   end-if
              when vdocum-tipo-nota 
                   move ef-numero-buf to como-numero
                   move low-value to tno-rec
                   move como-data to tno-data-doc
                   start tnotacr key >= tno-k-data
                         invalid   
                         start tnotacr key <= tno-k-data
                               invalid  continue
                           not invalid
                               perform until 1 = 2
                                 read tnotacr previous at end exit 
           perform end-read
                                 if tno-anno not = como-anno
                                    exit perform 
                                 end-if
                                 if tno-numero < como-numero
                                    if tco-data-doc > como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                                 if tno-numero > como-numero
                                    if tno-data-doc < como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                              end-perform
                         end-start
                     not invalid
                         perform until 1 = 2
                            read tnotacr next at end exit perform 
           end-read
                            if tno-anno not = como-anno
                               exit perform 
                            end-if
                            if tno-numero < como-numero
                               if tno-data-doc > como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                            if tno-numero > como-numero
                               if tno-data-doc < como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                         end-perform
                   end-start
                   if errori
                      move 78-ID-ef-data to store-id
                      set errori to true
                      move tno-data-doc to como-data
                      perform DATE-TO-SCREEN
                      move como-data to data-doc
                      display message "ESISTE NOTA CREDITO N. " 
           tno-numero
                               x"0d0a""CON DATA " data-doc
                                title tit-err
                                 icon 2
                   end-if
           end-evaluate.

           if tutto-ok
              if old-aliquota not = new-aliquota
                 set errori to true
                 display message "Impossibile cambiare cliente."
                          x"0d0a""Esenzione IVA differente"
                           title tit-err
                            icon 2
              end-if
           end-if.

           if errori
              perform CANCELLA-COLORE
              perform ABILITAZIONI
              move store-id to CONTROL-ID
              move 4        to ACCEPT-CONTROL
           else
              perform FORM1-BUF-TO-FLD
              perform CANCELLA-COLORE 
                         
              evaluate true
              when vdocum-tipo-fattura 
                   if old-tfa-cliente not = tfa-cliente
                      perform CANCELLA-RIFERIMENTO-FATTURA-SU-SCHEDA
                   else
                      if old-tfa-data-doc not = tfa-data-doc
                         perform CAMBIA-RIF-DATA-FATTURA-SU-SCHEDA
                      end-if
                   end-if
                   rewrite tfa-rec
              when vdocum-tipo-corrisp 
                   if old-tco-cliente not = tco-cliente
                      perform CANCELLA-RIFERIMENTO-CORRISP-SU-SCHEDA
                   else
                      if old-tco-data-doc not = tco-data-doc
                         perform CAMBIA-RIF-DATA-CORRISP-SU-SCHEDA
                      end-if
                   end-if
                   rewrite tco-rec
              when vdocum-tipo-nota    
                   rewrite tno-rec
              end-evaluate

              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1 
           .
      * <TOTEM:END>

       TORNA-IN-VISUA.
      * <TOTEM:PARA. TORNA-IN-VISUA>
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-data to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           unlock tfatture all records.
           unlock tcorrisp all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-OLD.
      * <TOTEM:PARA. VALORIZZA-OLD>
           move tfa-rec to old-tfa-rec.
           move tco-rec to old-tco-rec.
           move tno-rec to old-tno-rec.
           set vecchio to true 
           .
