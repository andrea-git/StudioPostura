           .

       Screen1-Event-Proc.
           .

       Screen1-Ta-1-Event-Proc.
           .

       Screen1-Gd-1-Event-Proc.
           EVALUATE Event-Type ALSO Event-Control-Id ALSO
                                    Event-Window-Handle
           WHEN Msg-Begin-Entry ALSO 5027 ALSO
                    form1-Handle 
              PERFORM gd-fatture-Ev-Msg-Begin-Entry
           WHEN Msg-Begin-Entry ALSO 32 ALSO
                    form1-Handle 
              PERFORM gd-default-Ev-Msg-Begin-Entry
           END-EVALUATE
           .

      * USER DEFINE PARAGRAPH
       ABILITAZIONI.
      * <TOTEM:PARA. ABILITAZIONI>
           if mod = 1              
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else           
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move ZERO to e-salva e-cancella
           end-if.      

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-nuovo, tool-salva, tool-cancella, tool-modifica.
      * FINE 
           .
      * <TOTEM:END>

       ANTEPRIMA.
      * <TOTEM:PARA. ANTEPRIMA>
           if nuovo
              display message "Salvare prima il Cliente"
                        title tit-err
                         icon 2
           else
              accept link-stampante from environment "STAMPANTE_ANTEPRIM
      -    "A"
              call   "st-cli-det" using cli-codice, link-stampante
              cancel "st-cli-det"
           end-if 
           .
      * <TOTEM:END>

       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = ZERO
              exit paragraph 
           end-if.
      *
           inquire gd-fatture, last-row in tot-righe.
           if tot-righe > 1
              display message "Impossibile cancellare cliente con docume
      -    "nti fiscali"
                        title tit-err
                         icon 2
              exit paragraph
           end-if.

           display message box MSg-Cancellare-il-record-corrente
                   title   titolo
                   type    mb-yes-no 
                   default mb-no
                   giving  scelta

           if scelta = mb-yes
              perform RIEMPI-CHIAVE
              delete clienti record invalid continue end-delete

              move 0    to mod
              move 1    to mod-k
              perform FORM1-CLEAR
              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC

              
              display message box 
           MSG-Cancellazione-avvenuta-con-successo
                      title titolo                         

              unlock clienti all records

              perform PRIMO 
              move 78-ID-ef-codice to  CONTROL-ID     
              move 4         to  ACCEPT-CONTROL 

              set Statusvisua to true
              perform STATUS-BAR-MSG
              modify TOOL-MODIFICA value = mod
           end-if 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate CONTROL-ID
           when 78-ID-ef-iva
                inquire ef-iva, value in iva-codice
                move 0 to iva-aliquota
                move "codiva-esente"   to Como-File
                call   "zoom-gt" using como-file, iva-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move iva-codice      to  ef-iva-buf
                   move iva-descrizione to lab-iva-buf
                   display ef-iva lab-iva
                   move 4 to accept-control
                end-if
      *                       
           when 78-ID-ef-tipo
                inquire ef-tipo, value in tcl-codice
                move "tipocli"      to Como-File
                call   "zoom-gt" using como-file, tcl-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tcl-codice      to  ef-tipo-buf
                   move tcl-descrizione to lab-tipo-buf
                   display ef-tipo lab-tipo
                   move 4 to accept-control
                end-if
           end-evaluate  
           .
      * <TOTEM:END>

       CLEAR-SCREEN.
      * <TOTEM:PARA. CLEAR-SCREEN>
           move cli-chiave to old-cli-chiave.

           initialize cli-dati replacing numeric data by zeroes
                                   alphanumeric data by spaces.
                         
           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.
                                          
           display form1. 

           perform RESETTA-GRID.
  
           unlock clienti all records 
           .
      * <TOTEM:END>

       CHANGE-TAB.
      * <TOTEM:PARA. CHANGE-TAB>
           set NonCambiareTab   to false.

           evaluate EVENT-DATA-1
           when 1
                perform CHECK-PAGE-2
                if tutto-ok
                   perform CANCELLA-COLORE
                   set ControllaCampi to true
                   move 78-ID-ef-ragsoc to control-id
                   move 4 to accept-control
                else
                   move 2 to pagina
                   set ControllaCampi to false    
                   move 4             to ACCEPT-CONTROL
                   set NonCambiareTab to true
                end-if
           when 2
                perform CHECK-PAGE-1
                if tutto-ok
                   perform CANCELLA-COLORE
                   set ControllaCampi to true
                else
                   move 1 to pagina
                   set ControllaCampi to false    
                   move 4             to ACCEPT-CONTROL
                   set NonCambiareTab to true
                end-if
           end-evaluate.

           display Screen1-Ta-1 
           .
      * <TOTEM:END>

       CHECK-INSERIMENTO.
      * <TOTEM:PARA. CHECK-INSERIMENTO>
      * MODIFICA LA STATUS-BAR
      * CONTROLLO SE DA INSERIMENTO SCORRO. ALLORA DEVO SEGNALARE CHE
      * SONO TORNATO IN MODIFICA.
           inquire tool-modifica, value in mod.

           evaluate mod
           when 1   set StatusModifica to true
           when 0   set StatusVisua    to true   
           end-evaluate.
      
           perform STATUS-BAR-MSG 
           .
      * <TOTEM:END>

       CHECK-CODFIS-PIVA.
      * <TOTEM:PARA. CHECK-CODFIS-PIVA>
           inquire ef-codfis   value cf-codice-fiscale.
           inquire ef-piva     value cf-piva.

           call   "codfis"     using link-codfis.
           cancel "codfis".

      * FLAG CHE INDICA SE IL CF O PIVA E' CORRETTO
           if cf-risultato not = ZERO
              set errori to true
           end-if 
           .
      * <TOTEM:END>

       CHECK-PAGE-1.
      * <TOTEM:PARA. CHECK-PAGE-1>
           if mod = 0 exit paragraph end-if.
           set tutto-ok to true.
           perform  varying control-id from 78-ID-ef-ragsoc by 1
                      until control-id    > 78-ID-ef-sconto2
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform 
           .
      * <TOTEM:END>

       CHECK-PAGE-2.
      * <TOTEM:PARA. CHECK-PAGE-2>
           if mod = 0 exit paragraph end-if.
           set tutto-ok to true.
      *****     perform  varying control-id from 78-ID-ef-marg by 1
      *****                until control-id    > 78-ID-ef-listino
      *****        perform CONTROLLO
      *****        if errori 
      *****           exit perform 
      *****        end-if
      *****     end-perform 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true. 
           if not ControllaCampi
              set ControllaCampi to true
              exit paragraph
           end-if.

           evaluate CONTROL-ID
           when 78-ID-ef-codice
                perform RIEMPI-CHIAVE
      
                if ef-codice-buf not = spaces
                   perform SELEZIONA-NUMERICO
                else
                   set errori to true
                   display message box MSG-Codice-obbligatorio
                           title tit-err
                           icon  mb-warning-icon
                end-if
      
           when 78-ID-ef-ragsoc
                inquire ef-ragsoc value in cli-ragsoc
                if cli-ragsoc = spaces
                   set errori to true
                   display message box "Ragione Sociale obbligatoria"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                end-if
                perform DESCRIZIONE-CLIENTE

           when 78-ID-ef-nome
                perform DESCRIZIONE-CLIENTE
      
           when 78-ID-ef-data-n
                inquire ef-data-n value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-n-buf
                   display ef-data-n
                end-if

           when 78-ID-ef-indirizzo
                inquire ef-indirizzo, value in cli-indirizzo
                if cli-indirizzo = spaces
                   set errori to true
                   display message "Indirizzo obbligatorio"
                             title tit-err
                              icon 2
                end-if   

           when 78-ID-ef-sesso
                inquire ef-sesso, value in ef-sesso-buf
                if ef-sesso-buf = "M" or
                   ef-sesso-buf = "F" or
                   ef-sesso-buf = "A"
                   continue
                else
                   set errori to true
                   display message "Valori ammessi M(aschio)/F(emmina)/A
      -    "(zienda)"
                             title tit-err
                              icon 2
                end-if  
      
           when 78-ID-ef-cap
                inquire ef-cap, value in cli-cap
                if cli-cap = SPACES
                   set errori to true
                   display message box "C.A.P. obbligatorio"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                end-if     
      
           when 78-ID-ef-localita     
                inquire ef-localita, value in cli-localita
                if cli-localita = spaces
                   set errori to true
                   display message box "Località obbligatoria"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                end-if     
      
           when 78-ID-ef-provincia                                      
               
                inquire ef-provincia, value in cli-prov
                if cli-prov = spaces
                   set errori to true
                   display message box "Provincia obbligatoria"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                end-if

           when 78-ID-ef-stato
                inquire ef-stato, value in cli-cittadinanza
                if cli-cittadinanza = spaces
                   set errori to true
                   display message "Cittadinanza obbligatoria"
                             title tit-err
                              type mb-ok
                              icon mb-warning-icon
                end-if 
      
           when 78-ID-ef-piva
                inquire ef-piva value in cli-piva
                if cli-piva not = spaces
                   perform CONTROLLO-PIVA
                   if tutto-ok
                      inquire ef-codfis value in ef-codfis-buf
                      if ef-codfis-buf = spaces
                         move ef-piva-buf to ef-codfis-buf
                      end-if
                      display ef-codfis
                   end-if
                end-if
      
           when 78-ID-ef-cee
                inquire ef-cee value in ef-cee-buf
                inquire ef-piva value in cli-piva
                if cli-piva = spaces
                   move ef-cee-buf to ef-piva-buf
                   display ef-piva
                end-if
      
           when 78-ID-ef-codfis
                inquire ef-piva,   value in cli-piva
                inquire ef-codfis, value in cli-codfis
                if ef-codfis-buf not = spaces
                   if cli-piva not = cli-codfis
                      perform CONTROLLO-CF
                   end-if
                else
                   move cli-piva to ef-codfis-buf
                   display ef-codfis
                end-if   
      
           when 78-ID-ef-iva
                inquire ef-iva      value in iva-codice
                move spaces  to iva-descrizione
                if iva-codice not = spaces
                   read codiva no lock
                        invalid
                        set errori to true
                        display message "Codice IVA errato"
                                  title tit-err
                                   icon 2
                    not invalid
                        if iva-aliquota not = 0
                           display message "Codice IVA NON esente"
                                     title tit-err
                                      icon 2
                           set errori to true
                        end-if
                   end-read
                end-if
                move iva-descrizione to lab-iva-buf
                display lab-iva 
      
           when 78-ID-ef-tipo
                inquire ef-tipo value in tcl-codice
                move spaces  to tcl-descrizione
                read tipocli no lock
                     invalid
                     set errori to true
                     display message "Codice tipologia errato"
                               title tit-err
                                icon 2
                 not invalid
                     inquire ef-sconto1 value in ef-sconto1-buf
                     move ef-sconto1-buf to cli-sconto1
                     if cli-sconto1 = 0
                        move tcl-sconto to ef-sconto1-buf
                        display ef-sconto1
                     end-if
                end-read
                move tcl-descrizione to lab-tipo-buf
                display lab-tipo

           when 78-ID-ef-sconto2
                inquire ef-sconto1 value in ef-sconto1-buf
                move ef-sconto1-buf to cli-sconto1
                inquire ef-sconto2 value in ef-sconto2-buf
                move ef-sconto2-buf to cli-sconto2
                if ( cli-sconto1 + cli-sconto2 ) > 100
                   display message "Totale sconti > 100"
                             title tit-err
                              icon 2
                   set errori to true
                end-if
                

           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if 
           .
      * <TOTEM:END>

       CONTROLLO-CF.
      * <TOTEM:PARA. CONTROLLO-CF>
           initialize link-codfis.
           set CF-CALCOLO to true.  
           move cli-codfis         to CF-CODICE-FISCALE.
           move ef-sesso-buf       to CF-SESSO.
           move ef-data-n-buf      to como-data.
           move como-data          to CF-DATA-N.
           move ef-nome-buf        to CF-NOME.
           move ef-ragsoc-buf      to CF-COGNOME.
           move ef-provincia-n-buf to CF-PROV.
           move ef-luogo-n-buf     to CF-LOC.

           call   "codfis" using LINK-CODFIS.
           cancel "codfis".

           if CF-CODICE-FISCALE not = ef-codfis-buf
              display message "Codice Fiscale NON valido."
                       x"0d0a""Proseguire comunque?"
                        title titolo
                         type mb-yes-no
                       giving scelta
                         icon 2
              if scelta = mb-no
                 set errori to true
              end-if
           end-if 
           .
      * <TOTEM:END>

       CONTROLLO-PIVA.
      * <TOTEM:PARA. CONTROLLO-PIVA>
           initialize link-codfis.
           set CF-PARTITA-IVA to true.
           move cli-piva to CF-PIVA.

           call   "codfis" using LINK-CODFIS.
           cancel "codfis".

           if cf-risultato not = 0
              display message "Partitva IVA non valida"
                       x"0d0a""Proseguire comunque?"
                        title titolo
                         type mb-yes-no
                       giving scelta
                         icon 2
              if scelta = mb-no
                 set errori to true
              end-if
           end-if 
           .
      * <TOTEM:END>

       CREA-CARTELLE.
      * <TOTEM:PARA. CREA-CARTELLE>
           accept  cartelle-clienti from environment "PATH_CLIENTI".
           inspect cartelle-clienti replacing trailing spaces by 
           low-value.
           initialize path-completo-cliente.
           string  cartelle-clienti delimited low-value 
                   cli-codice       delimited size
                   into path-completo-cliente
           end-string.
           move 0 to StatusCode.
           call "C$MAKEDIR" using path-completo-cliente
                           giving StatusCode.
           if StatusCode not = 0
              display message "Creazione cartella non riuscita!" 
                       x"0d0a" path-completo-cliente
                       x"0d0a""Procedere manualmente"
                        title tit-err
                         icon 2
           end-if 
           .
      * <TOTEM:END>

       CURRENT-RECORD.
      * <TOTEM:PARA. CURRENT-RECORD>
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if nuovo move 0 to mod end-if.

           if mod = 1
              read clienti    lock invalid 
                 set errori to true 
              end-read
           else
              read clienti no lock invalid 
                 set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0      to mod
                    move 1      to mod-k
                    move 78-ID-ef-codice to CONTROL-ID
                    move 4         to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else                         
                    set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG
                 perform VALORIZZA-OLD
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-codice to CONTROL-ID
                       move 4         to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                    end-if
                 end-if
              end-if
           end-if 
           .
      * <TOTEM:END>

       DESCRIZIONE-CLIENTE.
      * <TOTEM:PARA. DESCRIZIONE-CLIENTE>
           initialize lab-ragsoc-buf.
           inspect ef-ragsoc-buf replacing trailing spaces by low-value
           string  ef-ragsoc-buf delimited low-value
                   " "           delimited size
                   ef-nome-buf   delimited size
                   into lab-ragsoc-buf
           end-string.
           inspect ef-ragsoc-buf replacing trailing low-value by spaces.
           display lab-ragsoc 
           .
      * <TOTEM:END>

       INIT.
      * <TOTEM:PARA. INIT>
           move 0    to StatusHelp.
           move 1    to mod-k.
           set StatusVisua    to true. 
           set ControllaCampi to true 
           .
      * <TOTEM:END>

       INIT-OLD-REC.
      * <TOTEM:PARA. INIT-OLD-REC>
           initialize old-cli-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces 
           .
      * <TOTEM:END>

       MODIFICA.
      * <TOTEM:PARA. MODIFICA>
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if CONTROL-ID < 78-ID-ef-ragsoc
                       move 78-ID-ef-ragsoc to CONTROL-ID
                    end-if
                    move ZERO to mod-k
                    move 1    to mod
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1    to mod
                 perform SALV-MOD            
                 move ZERO to mod
                 move 1    to mod-k
                 if errori
                    move 1    to mod
                    move ZERO to mod-k
                 else
                    move 78-ID-ef-codice to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock clienti all records
                 end-if
              end-if
                        
              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if 
           .
      * <TOTEM:END>

       NUOVO.
      * <TOTEM:PARA. NUOVO>
           perform SALV-MOD.

           if tutto-ok  
              move 1         to mod mod-k
              modify tool-modifica,  value = mod
              perform CANCELLA-COLORE                  
              perform VALORIZZA-NUOVO
              perform INIT-OLD-REC       
              set StatusIns to true
              perform STATUS-BAR-MSG  
              set nuovo to true       
              unlock clienti all records
              move 78-ID-ef-codice to CONTROL-ID       
              move 4               to ACCEPT-CONTROL   
           end-if 
           .
      * <TOTEM:END>

       ORDINAMENTO.
      * <TOTEM:PARA. ORDINAMENTO>
           inquire tool-ord, value in OrderBy.
      
           if OrderBy = zero
              perform DataSet1-CHANGETO-KEY1
           else
              perform DataSet1-CHANGETO-KEY2
           end-if.
      
           perform CURRENT-RECORD 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".
           copy "status.cpy".
           copy "utydata.cpy" 
           .
      * <TOTEM:END>

       PRECEDENTE.
      * <TOTEM:PARA. PRECEDENTE>
           perform SALV-MOD.           
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock clienti all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-clienti-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-clienti-lock-flag
              end-if
                    
              perform CANCELLA-COLORE
              perform FORM1-PREVIOUS

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-clienti-lock-flag
                 move 1 to mod-k
                 perform FORM1-PREVIOUS
              end-if
           end-if 
           .
      * <TOTEM:END>

       PRIMO.
      * <TOTEM:PARA. PRIMO>
           perform SALV-MOD.           
           if tutto-ok    
 
              perform CHECK-INSERIMENTO
              unlock clienti all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-clienti-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-clienti-lock-flag
              end-if
                                  
              perform CANCELLA-COLORE
              perform FORM1-FIRST

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-clienti-lock-flag
                 move 1 to mod-k
                 perform FORM1-FIRST
              end-if
           end-if 
           .
      * <TOTEM:END>

       RESETTA-GRID.
      * <TOTEM:PARA. RESETTA-GRID>
           modify gd-fatture, reset-grid = 1.
           perform gd-fatture-CONTENT 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE.
      * <TOTEM:PARA. RIEMPI-CHIAVE>
           inquire ef-codice, value in ef-codice-buf.
           move ef-codice-buf       to cli-codice 
           .
      * <TOTEM:END>

       RIEMPI-GRID.
      * <TOTEM:PARA. RIEMPI-GRID>
           perform RIEMPI-GRID-FATTURE.
           perform RIEMPI-GRID-DEFAULT.

      ***---
       RIEMPI-GRID-FATTURE.
           move 0 to idx.
           perform RESETTA-GRID.
           move 2 to riga.
           move low-value  to tfa-rec.
           move cli-codice to tfa-cliente.
           start tfatture key >= tfa-k-cliente
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read tfatture next at end exit perform end-read
                    if tfa-cliente not = cli-codice
                       exit perform
                    end-if
                    move tfa-numero   to col-num
                    move tfa-data-doc to como-data
                    perform DATE-TO-SCREEN
                    move como-data    to col-data
                    move tfa-importo-sub  to col-sub
                    move tfa-importo-iva  to col-iva
                    move tfa-importo-tot  to col-tot
                    modify gd-fatture(riga, 1), hidden-data 1
                    modify gd-fatture(riga, 1), cell-data col-num
                    modify gd-fatture(riga, 2), cell-data col-data
                    modify gd-fatture(riga, 3), cell-data col-sub
                    modify gd-fatture(riga, 4), cell-data col-iva
                    modify gd-fatture(riga, 5), cell-data col-tot
                    modify gd-fatture(riga),    row-color = 481
                    add 1 to riga
                 end-perform
           end-start.
           move low-value  to tco-rec.
           move cli-codice to tco-cliente.
           start tcorrisp key >= tco-k-cliente
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read tcorrisp next at end exit perform end-read
                    if tco-cliente not = cli-codice
                       exit perform
                    end-if
                    move tco-numero   to col-num
                    move tco-data-doc to como-data
                    perform DATE-TO-SCREEN
                    move como-data       to col-data
                    move tco-importo-sub to col-sub
                    move tco-importo-iva to col-iva
                    move tco-importo-tot to col-tot
                    modify gd-fatture(riga, 1), hidden-data 2
                    modify gd-fatture(riga, 1), cell-data col-num
                    modify gd-fatture(riga, 2), cell-data col-data
                    modify gd-fatture(riga, 3), cell-data col-sub
                    modify gd-fatture(riga, 4), cell-data col-iva
                    modify gd-fatture(riga, 5), cell-data col-tot
                    modify gd-fatture(riga),    row-color = 193
                    add 1 to riga
                 end-perform
           end-start.

      ***---
       RIEMPI-GRID-DEFAULT.
           modify gd-default, reset-grid = 1.
           perform GD-DEFAULT-CONTENT.

           move 1 to idx.
           perform varying idx from 1 by 1 
                     until idx > 5
              add 1 to idx giving riga
              modify gd-default(riga, 1), cell-data cli-riga(idx)
           end-perform 
              
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           if mod = ZERO 
              exit paragraph 
           end-if.

           set tutto-ok to true.

           perform varying CONTROL-ID from 78-ID-ef-ragsoc by 1
                     until CONTROL-ID > 78-ID-ef-sconto2
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.          

           if tutto-ok
              if ef-piva-buf   = spaces and
                 ef-cee-buf    = spaces and
                 ef-codfis-buf = spaces
                 set errori to true
                 display message "Valorizzare almeno un dato fiscale per
      -    " la fatturazione"
                          x"0d0a""- Partita IVA"
                          x"0d0a""- Partita IVA CEE"
                          x"0d0a""- Codice Fiscale"
                           title tit-err
                            icon 2
                 perform CANCELLA-COLORE
                 move 78-ID-ef-piva to store-id
              end-if
           end-if.

           if tutto-ok 
              if ef-iva-buf not = spaces
                 display message "Il cliente verrà fatturato con IVA ese
      -    "nte."
                          x"0d0a""Confermi?"
                           title titolo
                            type mb-yes-no
                          giving scelta
                 if scelta = mb-no
                    set errori to true
                    perform CANCELLA-COLORE
                    move 78-ID-ef-iva to store-id
                 end-if
              end-if
           end-if.

           if errori
              perform ABILITAZIONI
              move store-id to CONTROL-ID
              move 4        to ACCEPT-CONTROL
           else                       
              if nuovo
                 perform CREA-CARTELLE
              end-if
              perform FORM1-BUF-TO-FLD
              perform CANCELLA-COLORE 
                                     
              accept como-data from century-date
              accept como-ora  from time
          
              write cli-rec invalid rewrite cli-rec end-write

              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD.

      *****     if SiSalvato and mod = 1
      *****        move 0 to idx
      *****        perform 10 times
      *****           add 1 to idx
      *****           if old-age-marca(idx) not = age-marca(idx)
      *****              set NoSalvato to true
      *****              move 78-ID-gd-marche to store-id
      *****              move 1 to colonna
      *****              add  1 to idx giving riga
      *****              exit perform
      *****           end-if
      *****           if old-age-perce-marca(idx) not = age-perce-marca(idx)
      *****              set NoSalvato to true
      *****              move 78-ID-gd-marche to store-id
      *****              add  1 to idx giving riga
      *****              move 3 to colonna
      *****              exit perform
      *****           end-if
      *****        end-perform
      *****     end-if.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel 
                            giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   inquire Screen1-Ta-1, value in pagina
                   evaluate pagina
                   when 1
                        if store-id > 78-ID-ef-note
                           move 666 to store-id
                        end-if
                   when 2
                        if store-id < 78-ID-gd-fatture
                           move 666 to store-id
                        end-if
                   end-evaluate
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA.
      * <TOTEM:PARA. SELEZIONA>
           perform RIEMPI-CHIAVE.

           move "clienti-alfa" to como-file.
           call "zoom-gt"   using como-file, cli-rec
                           giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = ZERO
              if old-cli-chiave not = cli-chiave or
               ( cli-codice = 0 and mod = 0 )
                 move cli-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to cli-chiave
                    set ReadSecca to true
                    modify ef-codice, value cli-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-ragsoc to CONTROL-ID       
                    move 4                 to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-ALFA.
      * <TOTEM:PARA. SELEZIONA-ALFA>
      * LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      * ANCORA GLI ZERI DAVANTI CHE ELIMINO
           inspect ef-codice-buf replacing leading ZERO by SPACES.
           move ef-codice-buf to cli-ragsoc.

      * POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      * ALTERNATA
           call "C$JUSTIFY" using cli-ragsoc, "L".

           start clienti key >= cli-ragsoc
                 invalid continue
             not invalid read clienti next 
           end-start.

           move "clienti-alfa" to como-file.
           call "zoom-gt" using como-file, cli-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = 0
              move cli-codice    to ef-codice-buf
              display ef-codice

              if old-cli-ragsoc not = cli-ragsoc
                 move cli-ragsoc to save-key-1
                 perform SALV-MOD
                 if tutto-ok
                    move save-key-1 to cli-ragsoc
                    set ReadSecca  to true
                    modify ef-codice, value cli-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-ragsoc to CONTROL-ID       
                    move 4               to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-NUMERICO.
      * <TOTEM:PARA. SELEZIONA-NUMERICO>
           read clienti no lock
                invalid
                if vecchio
                   perform SELEZIONA-ALFA
                end-if
            not invalid      
                if old-cli-chiave not = cli-chiave or
                 ( cli-codice = 0 and mod = 0 )
                   perform CURRENT-RECORD 
                end-if
           end-read 
           .
      * <TOTEM:END>

       STAMPA.
      * <TOTEM:PARA. STAMPA>
           if nuovo
              display message "Salvare prima il Cliente"
                        title tit-err
                         icon 2
           else
              move spaces to link-stampante
              call   "st-cli-det" using cli-codice, link-stampante
              cancel "st-cli-det"
           end-if 
           .
      * <TOTEM:END>

       SUCCESSIVO.
      * <TOTEM:PARA. SUCCESSIVO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock clienti all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-clienti-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-clienti-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-clienti-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if 
           .
      * <TOTEM:END>

       TORNA-IN-VISUA.
      * <TOTEM:PARA. TORNA-IN-VISUA>
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-codice to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock clienti all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if.

           set NonCambiareTab   to false.
           set ControllaCampi   to true.
           
           move 1 to event-data-1 screen1-ta-1-tab-value.
           perform SCREEN1-TA-1-TABCHANGE  
           .
      * <TOTEM:END>

       ULTIMO.
      * <TOTEM:PARA. ULTIMO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock clienti all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-clienti-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-clienti-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-clienti-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if 
           .
      * <TOTEM:END>

       VALORE-RIGA.
      * <TOTEM:PARA. VALORE-RIGA>
           inquire gd-fatture(riga, 1), cell-data in col-num.
           inquire gd-fatture(riga, 2), cell-data in col-data.
           inquire gd-fatture(riga, 3), cell-data in col-sub.
           inquire gd-fatture(riga, 4), cell-data in col-iva.
           inquire gd-fatture(riga, 5), cell-data in col-tot 
           .
      * <TOTEM:END>

       VALORIZZA-OLD.
      * <TOTEM:PARA. VALORIZZA-OLD>
           move cli-rec to old-cli-rec.
           set vecchio to true.

           move ZERO to StatusHelp
           perform STATUS-HELP 
           .
      * <TOTEM:END>

       VALORIZZA-NUOVO.
      * <TOTEM:PARA. VALORIZZA-NUOVO>
           move "00"    to status-clienti.

           initialize cli-rec replacing numeric data by zeroes
                                   alphanumeric data by spaces.
                                       
           perform FORM1-IUD-DISPLAY.

           move 1 to event-data-1 screen1-ta-1-tab-value.
           perform SCREEN1-TA-1-TABCHANGE  
           .
      * <TOTEM:END>

       X-Y.
      * <TOTEM:PARA. X-Y>
           inquire gd-fatture, cursor-y in riga,
                               cursor-x in colonna,
                               last-row in tot-righe 
           .
      * <TOTEM:END>

