BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if nuovo move 0 to mod end-if.

           if mod = 1
              evaluate true
              when vdocum-tipo-fattura
                   read tfatture lock invalid 
                        set errori to true 
                   end-read
              when vdocum-tipo-corrisp
                   read tcorrisp lock invalid 
                        set errori to true 
                   end-read
              when vdocum-tipo-nota
                   continue
              end-evaluate
           else
              evaluate true
              when vdocum-tipo-fattura
                   read tfatture no lock invalid 
                        set errori to true 
                   end-read
              when vdocum-tipo-corrisp
                   read tcorrisp no lock invalid 
                        set errori to true 
                   end-read
              when vdocum-tipo-nota
                   continue
              end-evaluate
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0      to mod
                    move 1      to mod-k
                    move 78-ID-ef-data to CONTROL-ID
                    move 4             to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-FLD-TO-BUF
                 display form1
                 set vecchio to true
      *****           if mod = 1
      *****              set StatusModifica to true
      *****           else                         
      *****              set StatusVisua    to true
      *****           end-if
      *****           perform STATUS-BAR-MSG
                 perform VALORIZZA-OLD
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-data to CONTROL-ID
                       move 4             to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = DESCRIZIONE-CLIENTE
BEGIN-TEXT
           initialize lab-cliente-buf.
           inspect cli-ragsoc replacing trailing spaces by low-value.
           string  cli-ragsoc delimited low-value
                   " "        delimited size
                   cli-nome   delimited size
              into lab-cliente-buf
           end-string.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-tfa-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.

           initialize old-tco-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.

           initialize old-tno-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1   to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    move 0   to mod-k
                    move 1   to mod
      *****              set StatusModifica to true
      *****              perform STATUS-BAR-MSG
                 end-if
              else
                 move 1 to mod
                 perform SALV-MOD            
                 move 0 to mod
                 move 1 to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    move 78-ID-ef-data to control-id
                    set NoMessage to true
                    perform CURRENT-RECORD
      *****              set StatusVisua to true
      *****              perform STATUS-BAR-MSG 
                    if vdocum-tipo-fattura
                       unlock tfatture all records
                    else
                       unlock tcorrisp all records
                    end-if
                 end-if
              end-if

              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if.

           if mod = 0
              move BitmapZoomEnabled to BitmapNumZoom
              move 0 to e-cerca
              modify tool-cerca, enabled e-cerca, 
                                 bitmap-number BitmapNumZoom
           else
              if tutto-ok
                 move 78-ID-ef-data to control-id
                 move 4 to accept-control
              end-if
           end-if.
END-TEXT
END-DATA
