                     perform DESCRIZIONE-CLIENTE
                end-read
                move tno-iva to iva-codice
                read codiva  no lock
                move iva-aliquota    to aliquota-z old-aliquota
                move aliquota-z      to lab-perce-buf
                call "C$JUSTIFY"  using lab-perce-buf, "L"
                inspect lab-perce-buf replacing leading x"30" by x"20"
                move lab-perce-buf   to lab-perce-buf (2:)
                move "("             to lab-perce-buf (1:1)
                move "%) ="          to lab-perce-buf (7:)
                move tno-importo-sub to lab-sub-buf 
                move tno-importo-iva to lab-iva-buf 
                move tno-importo-tot to lab-tot-buf 
                display form1
                modify cbo-pagamento reset-list 1
                modify cbo-pagamento, item-to-add = "Contanti"
                modify cbo-pagamento, item-to-add = "Assegno"
                modify cbo-pagamento, item-to-add = "Bonifico"
                modify cbo-pagamento, item-to-add = "POS"
                modify cbo-pagamento, value tno-pagamento
