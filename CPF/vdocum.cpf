BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           evaluate true
           when vdocum-tipo-fattura
                inquire ef-anno,   value in tfa-anno
                inquire ef-numero, value in tfa-numero
           when vdocum-tipo-corrisp
                inquire ef-anno,   value in tco-anno
                inquire ef-numero, value in tco-numero
           when vdocum-tipo-nota
                inquire ef-anno,   value in tno-anno
                inquire ef-numero, value in tno-numero
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           inquire form1-gd-1, last-row in tot-righe.
           if event-data-2 < 2
              move 2 to event-data-2
           end-if.
           if event-data-2 > tot-righe
              move tot-righe to event-data-2
           end-if.
           modify  form1-gd-1, start-y = event-data-2, 
                                     y = event-data-2,
                               start-x = 1, 
                                     x = 6,
                          region-color = 353.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           set SiSalvato to true.
           if mod = 0 exit paragraph end-if.
           perform Form1-BUF-TO-FLD.
           move 0 to scelta.
           evaluate true
           when vdocum-tipo-fattura
                if tfa-data-doc not = old-tfa-data-doc
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-data to store-id 
                end-if

                if tfa-cliente not = old-tfa-cliente
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-cliente to store-id 
                end-if

                if tfa-pagamento not = old-tfa-pagamento
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-pagamento to store-id 
                end-if

                if tfa-iban not = old-tfa-iban
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-iban to store-id 
                end-if

           when vdocum-tipo-corrisp
                if tco-data-doc not = old-tco-data-doc
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-data to store-id 
                end-if

                if tco-cliente not = old-tco-cliente
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-cliente to store-id 
                end-if

                if tco-pagamento not = old-tco-pagamento
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-pagamento to store-id 
                end-if

                if tco-iban not = old-tco-iban
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-iban to store-id 
                end-if

           when vdocum-tipo-nota  
                if tno-data-doc not = old-tno-data-doc
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-data to store-id 
                end-if

                if tno-cliente not = old-tno-cliente
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-ef-cliente to store-id 
                end-if

                if tno-pagamento not = old-tno-pagamento
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-pagamento to store-id 
                end-if

                if tno-iban not = old-tno-iban
                   and SiSalvato
                   set NoSalvato to true
                   move 78-ID-cbo-iban to store-id 
                end-if
           end-evaluate.

           if NoSalvato
              display message MSG-Salvare-le-modifiche
                        title titolo
                         type mb-yes-no-cancel 
                       giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           set tutto-ok to true.
           perform varying control-id from 78-ID-ef-data by 1 
                     until control-id > 78-ID-ef-cliente
              perform CONTROLLO
              if errori
                 exit perform
              end-if
           end-perform.

           if tutto-ok
              inquire ef-data, value in ef-data-buf
              move ef-data-buf to como-data
              perform DATE-TO-FILE
              move como-data(1:4) to como-anno
              set tutto-ok to true
              evaluate true
              when vdocum-tipo-fattura
                   move ef-numero-buf to como-numero
                   move low-value to tfa-rec
                   move como-data to tfa-data-doc
                   start tfatture key >= tfa-k-data
                         invalid  
                         start tfatture key <= tfa-k-data
                               invalid  continue
                           not invalid
                               perform until 1 = 2
                                 read tfatture previous at end exit perform end-read
                                 if tfa-anno not = como-anno
                                    exit perform 
                                 end-if
                                 if tfa-numero < como-numero
                                    if tfa-data-doc > como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                                 if tfa-numero > como-numero
                                    if tfa-data-doc < como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                              end-perform
                         end-start
                     not invalid
                         perform until 1 = 2
                            read tfatture next at end exit perform end-read
                            if tfa-anno not = como-anno
                               exit perform 
                            end-if
                            if tfa-numero < como-numero
                               if tfa-data-doc > como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                            if tfa-numero > como-numero
                               if tfa-data-doc < como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                         end-perform
                   end-start
                   if errori
                      move 78-ID-ef-data to store-id
                      move tfa-data-doc to como-data
                      perform DATE-TO-SCREEN
                      move como-data to data-doc
                      display message "ESISTE FATTURA N. " tfa-numero
                               x"0d0a""CON DATA " data-doc
                                title tit-err
                                 icon 2
                   end-if
              when vdocum-tipo-corrisp
                   move ef-numero-buf to como-numero
                   move low-value to tco-rec
                   move como-data to tco-data-doc
                   start tcorrisp key >= tco-k-data
                         invalid   
                         start tcorrisp key <= tco-k-data
                               invalid  continue
                           not invalid
                               perform until 1 = 2
                                 read tcorrisp previous at end exit perform end-read
                                 if tco-anno not = como-anno
                                    exit perform 
                                 end-if
                                 if tco-numero < como-numero
                                    if tco-data-doc > como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                                 if tco-numero > como-numero
                                    if tco-data-doc < como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                              end-perform
                         end-start
                     not invalid
                         perform until 1 = 2
                            read tcorrisp next at end exit perform end-read
                            if tco-anno not = como-anno
                               exit perform 
                            end-if
                            if tco-numero < como-numero
                               if tco-data-doc > como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                            if tco-numero > como-numero
                               if tco-data-doc < como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                         end-perform
                   end-start
                   if errori
                      move 78-ID-ef-data to store-id
                      set errori to true
                      move tco-data-doc to como-data
                      perform DATE-TO-SCREEN
                      move como-data to data-doc
                      display message "ESISTE CORRISPETTIVO N. " tco-numero
                               x"0d0a""CON DATA " data-doc
                                title tit-err
                                 icon 2
                   end-if
              when vdocum-tipo-nota 
                   move ef-numero-buf to como-numero
                   move low-value to tno-rec
                   move como-data to tno-data-doc
                   start tnotacr key >= tno-k-data
                         invalid   
                         start tnotacr key <= tno-k-data
                               invalid  continue
                           not invalid
                               perform until 1 = 2
                                 read tnotacr previous at end exit perform end-read
                                 if tno-anno not = como-anno
                                    exit perform 
                                 end-if
                                 if tno-numero < como-numero
                                    if tco-data-doc > como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                                 if tno-numero > como-numero
                                    if tno-data-doc < como-data
                                       set errori to true
                                       exit perform
                                    end-if
                                 end-if
                              end-perform
                         end-start
                     not invalid
                         perform until 1 = 2
                            read tnotacr next at end exit perform end-read
                            if tno-anno not = como-anno
                               exit perform 
                            end-if
                            if tno-numero < como-numero
                               if tno-data-doc > como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                            if tno-numero > como-numero
                               if tno-data-doc < como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                         end-perform
                   end-start
                   if errori
                      move 78-ID-ef-data to store-id
                      set errori to true
                      move tno-data-doc to como-data
                      perform DATE-TO-SCREEN
                      move como-data to data-doc
                      display message "ESISTE NOTA CREDITO N. " tno-numero
                               x"0d0a""CON DATA " data-doc
                                title tit-err
                                 icon 2
                   end-if
           end-evaluate.

           if tutto-ok
              if old-aliquota not = new-aliquota
                 set errori to true
                 display message "Impossibile cambiare cliente."
                          x"0d0a""Esenzione IVA differente"
                           title tit-err
                            icon 2
              end-if
           end-if.

           if errori
              perform CANCELLA-COLORE
              perform ABILITAZIONI
              move store-id to CONTROL-ID
              move 4        to ACCEPT-CONTROL
           else
              perform FORM1-BUF-TO-FLD
              perform CANCELLA-COLORE 
                         
              evaluate true
              when vdocum-tipo-fattura 
                   if old-tfa-cliente not = tfa-cliente
                      perform CANCELLA-RIFERIMENTO-FATTURA-SU-SCHEDA
                   else
                      if old-tfa-data-doc not = tfa-data-doc
                         perform CAMBIA-RIF-DATA-FATTURA-SU-SCHEDA
                      end-if
                   end-if
                   rewrite tfa-rec
              when vdocum-tipo-corrisp 
                   if old-tco-cliente not = tco-cliente
                      perform CANCELLA-RIFERIMENTO-CORRISP-SU-SCHEDA
                   else
                      if old-tco-data-doc not = tco-data-doc
                         perform CAMBIA-RIF-DATA-CORRISP-SU-SCHEDA
                      end-if
                   end-if
                   rewrite tco-rec
              when vdocum-tipo-nota    
                   rewrite tno-rec
              end-evaluate

              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-data to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           unlock tfatture all records.
           unlock tcorrisp all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if.
END-TEXT
