BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ABILITAZIONI
BEGIN-TEXT
           if mod = 1
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move 0 to e-salva e-cancella
           end-if.

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-salva, tool-cancella, tool-modifica.
      * FINE.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA
BEGIN-TEXT
           if mod = 0
              exit paragraph 
           end-if.
      *
           display message box MSG-Cancellare-il-record-corrente
                   title   titolo
                   type    mb-yes-no 
                   default mb-no
                   giving  scelta

           if scelta = mb-yes
              move low-value to rsc-rec
              perform RIEMPI-CHIAVE
              evaluate true
              when vdocum-tipo-fattura
                   delete tfatture record invalid continue end-delete

                   move low-value  to rfa-rec
                   move tfa-chiave to rfa-chiave-testa
                   start rfatture key >= rfa-chiave
                         invalid continue
                     not invalid
                         perform until 1 = 2
                            read rfatture next at end exit perform end-read
                            if rfa-chiave-testa not = tfa-chiave
                               exit perform
                            end-if
                            delete rfatture record
                         end-perform
                   end-start                           
                   perform CANCELLA-RIFERIMENTO-FATTURA-SU-SCHEDA
              when vdocum-tipo-corrisp
                   delete tcorrisp record invalid continue end-delete

                   move low-value  to rco-rec
                   move tco-chiave to rco-chiave-testa
                   start rcorrisp key >= rco-chiave
                         invalid continue
                     not invalid
                         perform until 1 = 2
                            read rcorrisp next at end exit perform end-read
                            if rco-chiave-testa not = tco-chiave
                               exit perform
                            end-if
                            delete rcorrisp record
                         end-perform
                   end-start
                   perform CANCELLA-RIFERIMENTO-CORRISP-SU-SCHEDA
              when vdocum-tipo-nota
                   delete tnotacr record invalid continue end-delete   

                   move low-value  to rno-rec
                   move tno-chiave to rno-chiave-testa
                   start rnotacr key >= rno-chiave
                         invalid continue
                     not invalid
                         perform until 1 = 2
                            read rnotacr next at end exit perform end-read
                            if rno-chiave-testa not = tno-chiave
                               exit perform
                            end-if
                            delete rnotacr record
                         end-perform
                   end-start
              end-evaluate 

              move 0    to mod
              move 1    to mod-k
              perform FORM1-CLEAR
              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC
              
              display message box MSG-Cancellazione-avvenuta-con-successo
                      title titolo                         

              unlock tfatture all records
              unlock tcorrisp all records
              unlock tnotacr  all records

              move 27 to key-status
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA-CAMBIA-RIFERIMENTI
BEGIN-TEXT
      ***---
       CANCELLA-RIFERIMENTO-FATTURA-SU-SCHEDA.
           move tfa-numero   to rsc-numfat.
           move tfa-data-doc to rsc-datafat.
           start rschede key >= rsc-k-fatcli
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read rschede next at end exit perform end-read
                    if rsc-numfat  not = tfa-numero or
                       rsc-datafat not = tfa-data-doc
                       exit perform
                    end-if
                    if rsc-cliente = old-tfa-cliente
                       move 0 to rsc-numfat
                       move 0 to rsc-datafat
                       rewrite rsc-rec
                    end-if
                 end-perform
           end-start.

      ***---
       CANCELLA-RIFERIMENTO-CORRISP-SU-SCHEDA.
           move tco-numero   to rsc-numcor.
           move tco-data-doc to rsc-datacor.
           start rschede key >= rsc-k-corcli
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read rschede next at end exit perform end-read
                    if rsc-numcor  not = tco-numero or
                       rsc-datacor not = tco-data-doc
                       exit perform
                    end-if
                    if rsc-cliente = old-tco-cliente
                       move 0 to rsc-numcor
                       move 0 to rsc-datacor
                       rewrite rsc-rec
                    end-if
                 end-perform
           end-start.

      ***---
       CAMBIA-RIF-DATA-FATTURA-SU-SCHEDA.
           move tfa-numero       to rsc-numfat.
           move old-tfa-data-doc to rsc-datafat.
           start rschede key >= rsc-k-fatcli
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read rschede next at end exit perform end-read
                    if rsc-numfat  not = tfa-numero or
                       rsc-datafat not = old-tfa-data-doc
                       exit perform
                    end-if
                    if rsc-cliente = old-tfa-cliente
                       move tfa-data-doc to rsc-datafat
                       rewrite rsc-rec
                    end-if
                 end-perform
           end-start.

      ***---
       CAMBIA-RIF-DATA-CORRISP-SU-SCHEDA.
           move tco-numero       to rsc-numcor.
           move old-tco-data-doc to rsc-datacor.
           start rschede key >= rsc-k-corcli
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read rschede next at end exit perform end-read
                    if rsc-numcor  not = tco-numero or
                       rsc-datacor not = old-tco-data-doc
                       exit perform
                    end-if
                    if rsc-cliente = old-tco-cliente
                       move tco-data-doc to rsc-datacor
                       rewrite rsc-rec
                    end-if
                 end-perform
           end-start.
END-TEXT
END-DATA
