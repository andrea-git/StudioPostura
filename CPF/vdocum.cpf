BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterBufToFld
BEGIN-TEXT
           evaluate true
           when vdocum-tipo-fattura
                move ef-cliente-buf to tfa-cliente
                move ef-data-buf    to como-data
                perform DATE-TO-FILE
                move como-data         to tfa-data-doc
                move cbo-pagamento-buf to tfa-pagamento
                move cbo-iban-buf      to tfa-iban
                
                move OLD-tfa-iva            to tfa-iva           
                move OLD-tfa-importo-sub    to tfa-importo-sub   
                move OLD-tfa-importo-iva    to tfa-importo-iva   
                move OLD-tfa-importo-tot    to tfa-importo-tot   
                move OLD-tfa-data-creazione to tfa-data-creazione

                perform varying riga from 2 by 1 
                          until riga > 6
                   subtract 1 from riga giving idx
                   inquire gd-note(riga, 1), cell-data in tfa-nota(idx)
                end-perform
                
           when vdocum-tipo-corrisp
                move ef-cliente-buf to tco-cliente
                move ef-data-buf    to como-data
                perform DATE-TO-FILE
                move como-data         to tco-data-doc
                move cbo-pagamento-buf to tco-pagamento
                move cbo-iban-buf      to tco-iban
                
                move OLD-tco-iva            to tco-iva           
                move OLD-tco-importo-sub    to tco-importo-sub   
                move OLD-tco-importo-iva    to tco-importo-iva   
                move OLD-tco-importo-tot    to tco-importo-tot   
                move OLD-tco-data-creazione to tco-data-creazione

                perform varying riga from 2 by 1 
                          until riga > 6
                   subtract 1 from riga giving idx
                   inquire gd-note(riga, 1), cell-data in tco-nota(idx)
                end-perform

           when vdocum-tipo-nota   
                move ef-cliente-buf to tno-cliente
                move ef-data-buf    to como-data
                perform DATE-TO-FILE
                move como-data         to tno-data-doc
                move cbo-pagamento-buf to tno-pagamento
                move cbo-iban-buf      to tno-iban
                
                move OLD-tno-iva            to tno-iva           
                move OLD-tno-importo-sub    to tno-importo-sub   
                move OLD-tno-importo-iva    to tno-importo-iva   
                move OLD-tno-importo-tot    to tno-importo-tot   
                move OLD-tno-data-creazione to tno-data-creazione

                perform varying riga from 2 by 1 
                          until riga > 6
                   subtract 1 from riga giving idx
                   inquire gd-note(riga, 1), cell-data in tno-nota(idx)
                end-perform
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
