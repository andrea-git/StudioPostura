                         read rcorrisp next at end exit perform end-read
                         if rco-chiave-testa not = tco-chiave
                            exit perform
                         end-if
                         move rco-articolo    to col-art
                         move rco-descrizione to col-des    
                         move rco-qta         to col-qta
                         move rco-listino     to col-listino
                         move rco-sconto      to col-sconto
                         move rco-prz         to col-prz
                         modify form1-gd-1(riga, 1), cell-data col-art
                         modify form1-gd-1(riga, 2), cell-data col-des    
                         modify form1-gd-1(riga, 3), cell-data col-qta
                         modify form1-gd-1(riga, 4), cell-data col-listino
                         modify form1-gd-1(riga, 5), cell-data col-sconto
                         modify form1-gd-1(riga, 6), cell-data col-prz
                         add 1 to riga
                      end-perform
                end-start    
                perform varying idx from 1 by 1 
                          until idx > 5
                   add 1 to idx giving riga
                   modify gd-note(riga, 1), cell-data tco-nota(idx)
                end-perform

           when vdocum-tipo-nota
                move "NOTA CREDITO"  to lab-docum-buf
                move vdocum-chiave   to tno-chiave
                read tnotacr  no lock
                move tno-anno       to ef-anno-buf
                move tno-numero     to ef-numero-buf
                move tno-data-doc   to como-data
                perform DATE-TO-SCREEN
                move como-data      to ef-data-buf
                move tno-cliente    to ef-cliente-buf cli-codice
                read clienti no lock
                     invalid
                     move "*** NON TROVATO ***" to lab-cliente-buf
