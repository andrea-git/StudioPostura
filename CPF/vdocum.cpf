BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-data
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-buf
                display ef-data
               
           when 78-ID-ef-cliente
                inquire ef-cliente, value in cli-codice
                read clienti no lock                   
                     invalid
                     set errori to true
                     display message "Cliente NON valido"
                               title tit-err
                                icon 2
                     move spaces to lab-cliente-buf
                 not invalid
                     perform DESCRIZIONE-CLIENTE
                     if cli-iva-esente not = spaces
                        move cli-iva-esente to iva-codice
                        read codiva no lock
                        move iva-aliquota to new-aliquota
                     else
                        move prm-iva to iva-codice
                        read codiva  no lock
                        move iva-aliquota to new-aliquota
                     end-if
                end-read
                display lab-cliente

           end-evaluate.

           if errori
              move 4 to accept-control
           end-if.
                
