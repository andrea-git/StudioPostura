BEGIN-TEXT
           move prm-codice to old-prm-codice.

           initialize prm-dati replacing numeric data by zeroes
                                   alphanumeric data by spaces.
                         
           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.
                                          
           perform DISPLAY-SCREEN.
  
           unlock param all records.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.
      
           evaluate CONTROL-ID
           when 78-ID-ef-anno
                inquire ef-anno, value in prm-anno
                if prm-anno = 0
                   display message "Anno d'esercizio obbligatorio"
                             title tit-err
                              icon 2
                   set errori to true
                end-if
      
           when 78-ID-ef-iban1
                inquire ef-iban1 value in prm-iban1
                if prm-iban1 = spaces
                   set errori to true
                   display message "IBAN obbligatorio"
                            title tit-err
                             icon 2
                end-if
      
           when 78-ID-ef-iva
                inquire ef-iva value in prm-iva
                move prm-iva to iva-codice
                move spaces  to iva-descrizione
                read codiva no lock
                     invalid
                     set errori to true
                     display message "Codice IVA errato"
                               title tit-err
                                icon 2
                 not invalid
                     if iva-aliquota = 0
                        display message "Codice IVA esente"
                                  title tit-err
                                   icon 2
                        set errori to true
                     end-if
                end-read
                move iva-descrizione to lab-iva-buf
                display lab-iva  
      
           when 78-ID-ef-ragsoc
                inquire ef-ragsoc value in prm-ragsoc
                if prm-ragsoc = spaces
                   set errori to true
                   display message "Ragione Sociale per intestazione fattura obbligatoria"
                            title tit-err
                             icon 2
                end-if  
      
           when 78-ID-ef-indirizzo
                inquire ef-indirizzo value in prm-indirizzo
                if prm-indirizzo = spaces
                   set errori to true
                   display message "Indirizzo per intestazione fattura obbligatorio"
                            title tit-err
                             icon 2
                end-if  
      
           when 78-ID-ef-citta
                inquire ef-citta value in prm-citta
                if prm-citta = spaces
                   set errori to true
                   display message "Citt per intestazione fattura obbligatorio"
                            title tit-err
                             icon 2
                end-if  
      
           when 78-ID-ef-cap
                inquire ef-cap value in prm-cap
                if prm-cap = spaces
                   set errori to true
                   display message "CAP per intestazione fattura obbligatorio"
                            title tit-err
                             icon 2
                end-if  
      
           when 78-ID-ef-prov
                inquire ef-prov value in prm-prov
                if prm-prov = spaces
                   set errori to true
                   display message "Provincia per intestazione fattura obbligatorio"
                            title tit-err
                             icon 2
                end-if  
      
           when 78-ID-ef-piva
                inquire ef-piva value in prm-piva
                if prm-piva = spaces
                   set errori to true
                   display message "Partita IVA per intestazione fattura obbligatorio"
                            title tit-err
                             icon 2
                else
                   perform CONTROLLO-PIVA
                   if tutto-ok
                      move ef-piva-buf to ef-codfis-buf
                   else
                      move spaces to ef-codfis-buf
                   end-if
                   display ef-codfis
                end-if  
      
           when 78-ID-ef-ragsoc
                inquire ef-ragsoc value in prm-ragsoc
                if prm-ragsoc = spaces
                   set errori to true
                   display message "Ragione Sociale per intestazione fattura obbligatorio"
                            title tit-err
                             icon 2
                end-if  
      
           when 78-ID-ef-data-f
                inquire ef-data-f value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-f-buf
                   display ef-data-f
                end-if  
      
           when 78-ID-ef-data-c
                inquire ef-data-c value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-c-buf
                   display ef-data-c
                end-if
      
           when 78-ID-ef-data-n
                inquire ef-data-n value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-n-buf
                   display ef-data-n
                end-if    
      
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move control-id to store-id
              move 4          to accept-control
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-PIVA
BEGIN-TEXT
           initialize link-codfis.
           set CF-PARTITA-IVA to true.
           move prm-piva to CF-PIVA.

           call   "codfis" using LINK-CODFIS.
           cancel "codfis".

           if cf-risultato not = 0
              display message "Partitva IVA non valida"
                       x"0d0a""Proseguire comunque?"
                        title titolo
                         type mb-yes-no
                       giving scelta
                         icon 2
              if scelta = mb-no
                 set errori to true
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if mod = 1
              read param    lock invalid set errori to true end-read
           else
              read param no lock invalid set errori to true end-read
           end-if.

           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0             to mod
                    move 1             to mod-k
                    move 78-ID-ef-anno to CONTROL-ID
                    move 4             to ACCEPT-CONTROL
                 end-if
                 perform FORM1-FLD-TO-BUF
                 perform DISPLAY-SCREEN
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else
                    set StatusVisua    to true
                 end-if
                 perform VALORIZZA-OLD
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-anno to CONTROL-ID
                       move 4             to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                                 title tit-err
                                  icon 2
                    end-if
                 end-if
              end-if
           end-if. 
           perform CHECK-INSERIMENTO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = DISPLAY-SCREEN
BEGIN-TEXT
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT
BEGIN-TEXT
           set vecchio  to true.
           set tutto-ok to true.
           
           move 0 to StatusHelp.
           move 1 to mod-k.
           set StatusVisua to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-prm-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           inquire tool-modifica, value in mod.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
              |SPECIFICO PER I PGM. CON GRID-CONTROL
              if mod = 1

                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if control-id < 78-ID-ef-anno
                       move 78-ID-ef-anno to control-id
                    end-if
                    move 0 to mod-k
                    move 1 to mod
                    set StatusModifica to true
                 end-if
              else
                 move 1 to mod
                 perform SALV-MOD            
                 move 0 to mod
                 move 1 to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    move 78-ID-ef-anno to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    unlock param all records
                    move 0     to mod
                 end-if
              end-if
                        
              perform DISPLAY-SCREEN
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to accept-control

           end-if.

           perform ABILITAZIONI.

           modify tool-modifica, value mod.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           set tutto-ok to true.

           perform SALV-MOD.

           if tutto-ok  
              move 78-ID-ef-anno to CONTROL-ID       
              move 4             to ACCEPT-CONTROL   
              move 1             to mod mod-k
              modify tool-modifica,  value = mod
              perform CANCELLA-COLORE                  
              perform VALORIZZA-NUOVO
              perform INIT-OLD-REC       
              set StatusIns to true
              perform STATUS-BAR-MSG
              set nuovo to true       
              unlock param all records
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".   
           copy "utydata.cpy".

      ***---
       STATUS-BAR-MSG.
           evaluate true
           when StatusModifica
                modify form1-st-1-handle, 
                       panel-index  3,
                       panel-text  "MODIFICA"
           when StatusVisua
                modify form1-st-1-handle, 
                       panel-index  3,
                       panel-text  "VISUALIZZAZIONE"
                move 0 to StatusHelp
                perform STATUS-HELP
           when StatusIns
                modify form1-st-1-handle, 
                       panel-index  3,
                       panel-text  "INSERIMENTO"
           when other
                modify form1-st-1-handle, 
                       panel-index  2,
                       panel-text   spaces
           end-evaluate.

      ***---
       STATUS-HELP.
           if StatusHelp = 1
              modify Form1-St-1-Handle, 
                     panel-index = 2, 
                     panel-text "F8 HELP record presenti"
              move BitmapZoomEnabled to BitmapNumZoom
           else
              modify Form1-St-1-Handle, 
                     panel-index = 2, 
                     panel-text spaces
              move BitmapZoomDisabled to BitmapNumZoom
           end-if.
           move StatusHelp to e-cerca.
           modify tool-cerca, enabled = e-cerca.
           modify tool-cerca, bitmap-number = BitmapNumZoom.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           move spaces to prm-codice.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD

           if NoSalvato
              display message MSG-Salvare-le-modifiche
                        title titolo
                         type mb-yes-no-cancel 
                       giving scelta       

              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.

           set tutto-ok to true.

           perform varying CONTROL-ID from 78-ID-ef-anno by 1
                     until CONTROL-ID > 78-ID-ef-data-n
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if tutto-ok
              set vecchio       to true

              perform FORM1-BUF-TO-FLD
              perform CANCELLA-COLORE 
              write prm-rec invalid rewrite prm-rec end-write
       
              set vecchio to true       
              perform TORNA-IN-VISUA            
           end-if.   
                 
           perform DISPLAY-SCREEN.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-anno to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           unlock param all records.         
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-NUOVO
BEGIN-TEXT
           move "00"    to status-param.

           initialize prm-rec replacing numeric data by zeroes
                                   alphanumeric data by spaces.
                                       
           perform FORM1-FLD-TO-BUF.
           perform DISPLAY-SCREEN.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move prm-rec to old-prm-rec.
           set vecchio to true.
END-TEXT
END-DATA
