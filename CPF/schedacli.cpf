BEGIN-TEXT
           if como-col-prz not = 0
              compute como-col-netto =
                      como-col-prz   -
                    ( como-col-prz   * como-col-sc1 / 100 )
      *****        compute como-col-netto =
      *****                como-col-netto - 
      *****              ( como-col-netto * como-col-sc2 / 100 )
           else
              move 0 to como-col-netto
           end-if.
