BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-stampa-LinkTo
BEGIN-TEXT
           if RigheSelezionate = 0
              display message "IMPOSSIBILE STAMPARE"
                       X"0D0A""NESSUNA RIGA SELEZIONATA."
                        title tit-err
                         icon 2
           else
              if PrezzoTotale = 0 
                 display message "IMPOSSIBILE STAMPARE DOCUMENTO A ZERO."
                           title tit-err
                            icon 2
              else
                 set trovato to false
                 perform FORM-STAMPA-OPEN-ROUTINE
                 if trovato
                    perform RIEMPI-GRID
                 end-if
              end-if
           end-if.


END-TEXT
