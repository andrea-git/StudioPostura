BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = cbo-documento-AfterProcedure
BEGIN-TEXT
      *<<** Customized_Default, SP-G, form-clien-Custom1-1 **>>
           inquire cbo-documento, value in cbo-documento-buf.
           if cbo-documento-buf = "Corrispettivo"
              if PagamentoFattura
                 modify cbo-pagamento, reset-list = 1
                 modify cbo-pagamento, item-to-add = "Contanti"
                 modify cbo-pagamento, value = "Contanti"
                 set PagamentoCorrisp to true
              end-if
           else
              if PagamentoCorrisp
                 modify cbo-pagamento, reset-list = 1
                 modify cbo-pagamento, item-to-add = "Contanti"
                 modify cbo-pagamento, item-to-add = "Assegno"
                 modify cbo-pagamento, item-to-add = "Bonifico"
                 modify cbo-pagamento, item-to-add = "POS"
                 modify cbo-pagamento, value = "Contanti"
                 set PagamentoFattura to true
              end-if
           end-if.
           move 78-ID-cbo-pagamento to control-id.
           move 4 to accept-control.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = cbo-pagamento-AfterProcedure
BEGIN-TEXT
      *<<** Customized_Default, SP-G, form-clien-Custom1-1 **>>
           move 78-ID-ef-data-stampa to control-id.
           move 4 to accept-control.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ef-data-stampa-BeforeProcedure
BEGIN-TEXT
      *<<** Customized_Default, SP-G, form-clien-Custom1-1 **>>
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ef-data-stampa-AfterProcedure
BEGIN-TEXT
      *<<** Customized_Default, SP-G, form-clien-Custom1-1 **>>
           inquire ef-data-stampa, value in ef-data-stampa-buf.
           move ef-data-stampa-buf to como-data.
           perform DATE-FORMAT.
           move como-data to ef-data-stampa-buf.
           display ef-data-stampa.
                                       
           perform DATE-TO-FILE.
           move como-data(1:4) to como-anno.
           set tutto-ok to true.
           if cbo-documento-buf = "Fattura"
              add 1 to prm-fatture-num giving como-numero
              move low-value to tfa-rec
              move como-data to tfa-data-doc
              start tfatture key >= tfa-k-data
                    invalid  
                    start tfatture key <= tfa-k-data
                          invalid  continue
                      not invalid
                          perform until 1 = 2
                            read tfatture previous at end exit perform end-read
                            if tfa-anno not = como-anno
                               exit perform 
                            end-if
                            if tfa-numero < como-numero
                               if tfa-data-doc > como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                            if tfa-numero > como-numero
                               if tfa-data-doc < como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                         end-perform
                    end-start
                not invalid
                    perform until 1 = 2
                       read tfatture next at end exit perform end-read
                       if tfa-anno not = como-anno
                          exit perform 
                       end-if
                       if tfa-numero < como-numero
                          if tfa-data-doc > como-data
                             set errori to true
                             exit perform
                          end-if
                       end-if
                       if tfa-numero > como-numero
                          if tfa-data-doc < como-data
                             set errori to true
                             exit perform
                          end-if
                       end-if
                    end-perform
              end-start
              if errori
                 move tfa-data-doc to como-data
                 perform DATE-TO-SCREEN
                 move como-data to data-doc
                 display message "ESISTE FATTURA N. " tfa-numero
                          x"0d0a""CON DATA " data-doc
                           title tit-err
                            icon 2 
              end-if
           else
              add 1 to prm-corrisp-num giving como-numero
              move low-value to tco-rec
              move como-data to tco-data-doc
              start tcorrisp key >= tco-k-data
                    invalid  
                    start tcorrisp key <= tco-k-data
                          invalid  continue
                      not invalid
                          perform until 1 = 2
                            read tcorrisp previous at end exit perform end-read
                            if tco-anno not = como-anno
                               exit perform 
                            end-if
                            if tco-numero < como-numero
                               if tco-data-doc > como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                            if tco-numero > como-numero
                               if tco-data-doc < como-data
                                  set errori to true
                                  exit perform
                               end-if
                            end-if
                         end-perform
                    end-start
                not invalid
                    perform until 1 = 2
                       read tcorrisp next at end exit perform end-read
                       if tco-anno not = como-anno
                          exit perform 
                       end-if
                       if tco-numero < como-numero
                          if tco-data-doc > como-data
                             set errori to true
                             exit perform
                          end-if
                       end-if
                       if tco-numero > como-numero
                          if tco-data-doc < como-data
                             set errori to true
                             exit perform
                          end-if
                       end-if
                    end-perform
              end-start
              
              if errori
                 set errori to true
                 move tco-data-doc to como-data
                 perform DATE-TO-SCREEN
                 move como-data to data-doc
                 display message "ESISTE CORRISPETTIVO N. " tco-numero
                          x"0d0a""CON DATA " data-doc
                           title tit-err
                            icon 2   
              end-if
           end-if.


           if tutto-ok
              move 78-ID-cbo-iban to control-id
           end-if.
           move 4 to accept-control.
END-TEXT
