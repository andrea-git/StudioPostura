   BASETYPE          = 2
   PARAGRAPH         = chk-certif-LinkTo
BEGIN-TEXT
           if chk-certif-buf = 0
              modify chk-certif, bitmap-number = 3
              inquire gd-note(3, 1), cell-data in col-note
              if col-note = 78-allegato-cm
                 modify gd-note(3, 1), cell-data spaces
              end-if   
           else
              modify chk-certif, bitmap-number = 4
              modify gd-note(3, 1), cell-data 78-allegato-cm 
              perform APRI-ALLEGATI-CLIENTE
           end-if.
END-TEXT
END-DATA
