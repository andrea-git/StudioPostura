BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = form-stampa
   ACTOBJ            = form-stampa
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           modify cbo-documento, value "Fattura".
           modify cbo-pagamento, value "Contanti".
           

           move spaces to prm-chiave.
           read param no lock.
           modify cbo-iban, item-to-add = prm-iban1.
           modify cbo-iban, item-to-add = prm-iban2.
           modify cbo-iban, value prm-iban1.

           move prm-fatture-data to como-data.
           perform DATE-TO-SCREEN.
           move como-data to ef-data-stampa-buf.
           display ef-data-stampa.

           move 78-ID-cbo-documento to control-id.
           move 4 to accept-control.

           set PagamentoFattura to true.

           move 0 to chk-scheda-buf.
           modify chk-scheda, value chk-scheda-buf, bitmap-number = 1.

           move 0 to chk-certif-buf.
           modify chk-certif, value chk-certif-buf, bitmap-number = 3.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = APRI-ALLEGATI-CLIENTE
BEGIN-TEXT
           accept  cartelle-clienti from environment "PATH_CLIENTI".
           inspect cartelle-clienti replacing trailing spaces by low-value.
           initialize path-completo-cliente.
           string  cartelle-clienti delimited low-value 
                   ef-clientea-buf  delimited size
                   into path-completo-cliente
           end-string.
                      
           initialize opensave-data.
           move path-completo-cliente to opnsav-default-dir.
                                    
           move "PDF files (*.pdf)|*.pdf|JPG files (*.jpg)|*.jpg|DOC files (*.doc)|*.doc|All files (*.*)|*.*" 
             to opnsav-filters.
           move 1 to opnsav-default-filter.
           move "Documentazione"   to opnsav-title.
           call "C$OPENSAVEBOX" using opensave-save-box, opensave-data
                               giving opensave-status.

           if opensave-status = 1
              initialize path-completo-cliente
              string "START "            delimited size
                    x"22222022"          delimited size
                     OPNSAV-FILENAME     delimited size
                    x"22"                delimited size
                    into path-completo-cliente
              end-string
           end-if.
           call "C$SYSTEM" using path-completo-cliente, 64
                          giving StatusCode.

           if StatusCode = -1
              display message "Comando fallito!"
                        title tit-err
                         icon 2
           end-if.
END-TEXT
END-DATA
