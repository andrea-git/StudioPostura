BEGIN-TEXT
           if chk-scheda-buf = 0
              modify chk-scheda, bitmap-number = 1
              inquire gd-note(2, 1), cell-data in col-note
              if col-note = 78-allegato-sc
                 modify gd-note(2, 1), cell-data spaces
              end-if   
           else
              modify chk-scheda, bitmap-number = 2
              modify gd-note(2, 1), cell-data 78-allegato-sc
              accept link-stampante from environment "STAMPANTE_ANTEPRIMA"
              call   "st-schedacli" using tsc-codice, link-stampante
              cancel "st-schedacli"
           end-if.
END-TEXT
