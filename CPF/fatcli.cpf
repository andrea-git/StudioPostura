BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd1-Ev-Msg-Begin-Entry
BEGIN-TEXT
           set  event-action to event-action-fail-terminate.
           inquire gd1, entry-reason in como-x
           evaluate como-x
           when X"00"|doppio click
           when X"0D"|invio
           when X"20"|space
                inquire gd1(event-data-2, 78-col-qta),   cell-data col-qta
                inquire gd1(event-data-2, 78-col-netto), cell-data col-netto
                move col-qta   to como-qta
                if como-qta = 0 move 1 to como-qta end-if
                move col-netto to como-netto
                inquire gd1(event-data-2, 1), hidden-data Hid-Sel
                if Hid-SiSel
                   set Hid-NoSel to true
                   modify gd1(event-data-2), 
                          row-font  Calibri12-Occidentale 
                          row-color 1
                   subtract 1 from RigheSelezionate
                   compute PrezzoTotale = 
                           PrezzoTotale - como-qta * como-netto
                else
                   set Hid-SiSel to true
                   modify gd1(event-data-2), 
                          row-font  FontSelezionato
                          row-color 5
                   add 1 to RigheSelezionate
                   compute PrezzoTotale = 
                           PrezzoTotale + como-qta * como-netto
                end-if
                modify gd1(event-data-2, 1), hidden-data Hid-Sel
                perform COLORE-RIGA
                move PrezzoTotale to ef-totale-buf
                display ef-totale
           end-evaluate.
END-TEXT
END-DATA
