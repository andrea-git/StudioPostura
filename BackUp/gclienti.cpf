   BASETYPE          = 2
   PARAGRAPH         = pb-codfis-LinkTo
BEGIN-TEXT
           initialize link-codfis.
           set CF-CALCOLO to true.
           move cli-codfis         to CF-CODICE-FISCALE.
           move ef-sesso-buf       to CF-SESSO.
           move ef-data-n-buf      to como-data.
           move como-data          to CF-DATA-N.
           move ef-nome-buf        to CF-NOME.
           move ef-ragsoc-buf      to CF-COGNOME.
           move ef-provincia-n-buf to CF-PROV.
           move ef-luogo-n-buf     to CF-LOC.

           call   "codfis" using LINK-CODFIS.
           cancel "codfis".

           move CF-CODICE-FISCALE to ef-codfis-buf.
           display ef-codfis.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-docum-LinkTo
BEGIN-TEXT
           accept  cartelle-clienti from environment "PATH_CLIENTI".
           inspect cartelle-clienti replacing trailing spaces by low-value.
           initialize path-completo-cliente.
           string  cartelle-clienti delimited low-value 
                   cli-codice       delimited size
                   into path-completo-cliente
           end-string.
                      
           initialize opensave-data.
           move path-completo-cliente to opnsav-default-dir.
                                    
           move "PDF files (*.pdf)|*.pdf|JPG files (*.jpg)|*.jpg|DOC files (*.doc)|*.doc|All files (*.*)|*.*" 
             to opnsav-filters.
           move 1 to opnsav-default-filter.
           move "Documentazione"   to opnsav-title.
           call "C$OPENSAVEBOX" using opensave-save-box, opensave-data
                               giving opensave-status.

           if opensave-status = 1
              initialize path-completo-cliente
              string "START "            delimited size
                    x"22222022"          delimited size
                     OPNSAV-FILENAME     delimited size
                    x"22"                delimited size
                    into path-completo-cliente
              end-string
           end-if.
           call "C$SYSTEM" using path-completo-cliente, 64
                          giving StatusCode.

           if StatusCode = -1
              display message "Comando fallito!"
                        title tit-err
                         icon 2
           end-if.
END-TEXT
END-DATA
