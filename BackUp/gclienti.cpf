BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-immagine-LinkTo
BEGIN-TEXT
           accept  cartelle-clienti from environment "PATH_CLIENTI".
           inspect cartelle-clienti replacing trailing spaces by low-value.
           initialize path-completo-cliente.
           string  cartelle-clienti delimited low-value 
                   cli-codice       delimited size
                   into path-completo-cliente
           end-string.
                      
           initialize opensave-data.
           move path-completo-cliente to opnsav-default-dir.
                                    
           move "Bitmap files (*.bmp)|*.bmp|JPG files (*.jpg)|*.jpg" 
             to opnsav-filters.
           move 1 to opnsav-default-filter.
           move "Documentazione"   to opnsav-title.
           call "C$OPENSAVEBOX" using opensave-save-box, opensave-data
                               giving opensave-status.

           if opensave-status = 1
              destroy contact-bmp
              move 0 to contact-bmp
              
              CALL "w$bitmap" USING WBITMAP-LOAD OPNSAV-FILENAME, 
                             GIVING contact-bmp
              if contact-bmp > 999999
                 move default-bmp to default-bmp
              end-if
              modify bmp-contact, bitmap-handle contact-bmp
              move OPNSAV-FILENAME to ef-foto-buf
              display ef-foto
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd-fatture-Ev-Msg-Begin-Entry
BEGIN-TEXT
           set event-action to event-action-fail-terminate.
           inquire gd-fatture, entry-reason in como-x
           evaluate como-x
           when X"00"|doppio click
           when X"0D"|invio
                inquire gd-fatture(event-data-2, 1), cell-data in col-num
                move col-num to tfa-numero
                if tfa-numero not = 0
      *****                   move OLD-col-numdoc  to tfa-numero
      *****                   move OLD-col-datadoc to como-data
      *****                   perform DATE-TO-FILE
      *****                   move como-data(1:4)  to tfa-numero
                   set VediFattura to true
                end-if
           end-evaluate.
END-TEXT
END-DATA
