BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ADD-GIORNO
BEGIN-TEXT
           compute como-numero = function integer-of-date(como-data).
           add 1 to como-numero.
           compute como-data = function date-of-integer(como-numero).

           perform TROVA-GIORNO.
           if giorno = 7
              compute como-numero = function integer-of-date(como-data)
              add 1 to como-numero
              compute como-data = function date-of-integer(como-numero)
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
