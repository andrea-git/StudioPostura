           inquire gd-cerca, entry-reason in como-entry
           if como-entry = x"13" or x"20" or x"0d" or x"00"        
              if event-data-1 = 6
                 set ModificaInLine       to true
              else
                 set ModificaAppuntamento to true
              end-if
              inquire gd-cerca(event-data-2, 1), hidden-data sav-chiave
           end-if.
END-TEXT
