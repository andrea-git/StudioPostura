                 perform until 1 = 2
                    read appuntamenti next at end exit perform end-read
                    if not (app-test) or 
                       app-data > como-data-a
                       exit perform
                    end-if

                    move 0 to sw-agisci
                    if ef-cliente-t-buf = spaces
