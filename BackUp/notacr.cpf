BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CALCOLA-TOTALE
BEGIN-TEXT
           move 0 to importo-sub.
           move 0 to importo-iva.
           move 0 to importo-tot.

           inquire form1-gd-1, last-row in tot-righe.
           perform varying store-riga from 2 by 1 
                     until store-riga > tot-righe
              inquire form1-gd-1(store-riga, 4), cell-data in como-qta
              inquire form1-gd-1(store-riga, 5), cell-data in como-prz
              if como-qta = 0
                 move 1 to como-qta
              end-if
              compute importo-sub = 
                      importo-sub + como-qta * como-prz
           end-perform.
           move save-iva to iva-codice.
           read codiva no lock.

           compute iva-3dec = importo-sub * iva-aliquota / 100
           add 0,005     to iva-3dec giving iva-2dec
           move iva-2dec to importo-iva
           compute importo-tot = 
                   importo-sub + 
                   importo-iva.

           move importo-sub to lab-sub-buf.
           move importo-iva to lab-iva-buf.
           move importo-tot to lab-tot-buf.
           display lab-sub lab-iva lab-tot.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-cliente
                inquire ef-cliente, value in cli-codice
                move "clienti-alfa" to como-file
                call "zoom-gt"   using como-file, cli-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   perform DESCRIZIONE-CLIENTE
                   display lab-cliente                        
                   move cli-codice to ef-cliente-buf
                   display ef-cliente
                end-if
           when 78-ID-form1-gd-1
                inquire form1-gd-1(riga, 1), cell-data in art-codice
                move "articoli"     to Como-File
                call   "zoom-gt" using  como-file, art-rec
                                 giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move art-codice        to col-art
                   move art-descrizione   to col-des
                   modify form1-gd-1(riga, 1), cell-data = col-art
                   modify form1-gd-1(riga, 2), cell-data = col-des
                   perform CONTROLLO-RIGA
                end-if
                
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-data
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-buf
                display ef-data
               
           when 78-ID-ef-cliente
                inquire ef-cliente, value in cli-codice
                read clienti no lock                   
                     invalid
                     set errori to true
                     display message "Cliente NON valido"
                               title tit-err
                                icon 2
                     move spaces to lab-cliente-buf
                 not invalid
                     perform DESCRIZIONE-CLIENTE
                     if cli-iva-esente not = spaces
                        move cli-iva-esente to save-iva
                     end-if
                end-read
                display lab-cliente

           end-evaluate.

           if errori
              move 4 to accept-control
           end-if.
                
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-RIGA
BEGIN-TEXT
           set tutto-ok to true.  

           evaluate colonna
           when 1
                move col-art to art-codice
                if art-codice not = spaces
                   read articoli no lock
                        invalid
                        move spaces to art-descrizione
                        set errori to true
                        display message "Articolo NON valido"
                                  title tit-err
                                icon 2
                   end-read
                   move art-descrizione to col-des
                   modify form1-gd-1(riga, 2), cell-data col-des
                end-if
           when 2
                if col-art = spaces and col-des = spaces
                   set errori to true
                   display message "Articolo o Descrizione obbligatorio"
                             title tit-err
                              icon 2
                end-if

           when 3
                perform CALCOLA-TOTALE

           when 4
                if como-prz = 0 and col-art not = spaces
                   set errori to true
                   display message "Prezzo obbligatorio"
                             title tit-err
                              icon 2
                end-if
                if tutto-ok
                   perform CALCOLA-TOTALE
                end-if
      
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = DESCRIZIONE-CLIENTE
BEGIN-TEXT
           initialize lab-cliente-buf.
           inspect cli-ragsoc replacing trailing spaces by low-value.
           string  cli-ragsoc delimited low-value
                   " "        delimited size
                   cli-nome   delimited size
              into lab-cliente-buf
           end-string.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
