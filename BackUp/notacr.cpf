BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           move BitmapZoomEnabled to BitmapNumZoom.
           move 0 to e-cerca.
           modify tool-cerca, enabled e-cerca, 
                              bitmap-number BitmapNumZoom.
           
           move spaces  to prm-chiave.
           read param   no lock.
           move prm-iva to save-iva.
           add 1               to prm-notecr-num.
           move prm-notecr-num to como-numero.
           modify cbo-pagamento, value = "Contanti".
           
           perform RESETTA-GRID.
           evaluate true
           when vdocum-tipo-fattura
                move vdocum-chiave to tfa-chiave
                read tfatture   no lock
                     invalid continue |NON PASSO IL DOCUMENTO
                 not invalid
                     move tfa-iva       to save-iva
                     move "FATTURA"     to lab-docum-buf
                     move tfa-anno      to ef-anno-buf
                     move tfa-numero    to ef-numero-buf
                     move tfa-data-doc  to como-data
                     perform DATE-TO-SCREEN
                     move como-data     to ef-data-buf
                     move tfa-cliente   to ef-cliente-buf cli-codice
                     read clienti no lock
                          invalid
                          move "*** NON TROVATO ***" to lab-cliente-buf
                      not invalid
                          perform DESCRIZIONE-CLIENTE
                     end-read
                     move tfa-iva to iva-codice
                     read codiva  no lock
                     move iva-aliquota    to aliquota-z
                     move aliquota-z      to lab-perce-buf
                     call "C$JUSTIFY"  using lab-perce-buf, "L"
                     inspect lab-perce-buf 
                             replacing leading x"30" by x"20"
                     move lab-perce-buf   to lab-perce-buf (2:)
                     move "("             to lab-perce-buf (1:1)
                     move "%) ="          to lab-perce-buf (7:)
                     move tfa-importo-sub to lab-sub-buf 
                     move tfa-importo-iva to lab-iva-buf 
                     move tfa-importo-tot to lab-tot-buf 
                     display form1
                     modify cbo-pagamento  reset-list 1
                     modify cbo-pagamento, item-to-add = "Contanti"
                     modify cbo-pagamento, item-to-add = "Assegno"
                     modify cbo-pagamento, item-to-add = "Bonifico"
                     modify cbo-pagamento, value tfa-pagamento
                     modify cbo-iban,      value tfa-iban
                     move low-value to rfa-rec
                     move tfa-chiave to rfa-chiave-testa
                     start rfatture key >= rfa-chiave
                           invalid continue
                       not invalid
                           move 2 to riga
                           perform until 1 = 2
                              read rfatture next at end exit perform end-read
                              if rfa-chiave-testa not = tfa-chiave
                                 exit perform
                              end-if
                              move rfa-articolo    to col-art
                              move rfa-descrizione to col-des
                              move rfa-listino     to col-listino
                              move rfa-qta         to col-qta
                              move rfa-prz         to col-prz
                              modify form1-gd-1(riga, 1), cell-data col-art
                              modify form1-gd-1(riga, 2), cell-data col-des
                              modify form1-gd-1(riga, 3), cell-data col-listino
                              modify form1-gd-1(riga, 4), cell-data col-qta
                              modify form1-gd-1(riga, 5), cell-data col-prz
                              add 1 to riga
                           end-perform
                     end-start
                     perform varying idx from 1 by 1 
                               until idx > 5
                        add 1 to idx giving riga
                        modify gd-note(riga, 1), cell-data tfa-nota(idx)
                     end-perform
                end-read

           when vdocum-tipo-corrisp
                move vdocum-chiave   to tco-chiave
                read tcorrisp  no lock
                     invalid continue |NON PASSO IL DOCUMENTO
                 not invalid
                     move tco-iva       to save-iva
                     move "CORRISPETTIVO" to lab-docum-buf
                     move tco-anno       to ef-anno-buf
                     move tco-numero     to ef-numero-buf
                     move tco-data-doc   to como-data
                     perform DATE-TO-SCREEN
                     move como-data      to ef-data-buf
                     move tco-cliente    to ef-cliente-buf cli-codice
                     read clienti no lock
                          invalid
                          move "*** NON TROVATO ***" to lab-cliente-buf
                      not invalid
                          perform DESCRIZIONE-CLIENTE
                     end-read
                     move tco-iva to iva-codice
                     read codiva  no lock
                     move iva-aliquota    to aliquota-z
                     move aliquota-z      to lab-perce-buf
                     call "C$JUSTIFY"  using lab-perce-buf, "L"
                     inspect lab-perce-buf 
                             replacing leading x"30" by x"20"
                     move lab-perce-buf   to lab-perce-buf (2:)
                     move "("             to lab-perce-buf (1:1)
                     move "%) ="          to lab-perce-buf (7:)
                     move tco-importo-sub to lab-sub-buf 
                     move tco-importo-iva to lab-iva-buf 
                     move tco-importo-tot to lab-tot-buf 
                     display form1
                     modify cbo-pagamento reset-list 1
                     modify cbo-pagamento, item-to-add = "Contanti"
                     modify cbo-pagamento, value tco-pagamento
                     modify cbo-iban,      value tco-iban
                     move low-value to rco-rec
                     move tco-chiave to rco-chiave-testa
                     start rcorrisp key >= rco-chiave
                           invalid continue
                       not invalid
                           move 2 to riga
                           perform until 1 = 2
                              read rcorrisp next at end exit perform end-read
                              if rco-chiave-testa not = tco-chiave
                                 exit perform
                              end-if
                              move rco-articolo    to col-art
                              move rco-descrizione to col-des  
                              move rco-listino     to col-listino
                              move rco-qta         to col-qta
                              move rco-prz         to col-prz
                              modify form1-gd-1(riga, 1), cell-data col-art
                              modify form1-gd-1(riga, 2), cell-data col-des
                              modify form1-gd-1(riga, 3), cell-data col-listino
                              modify form1-gd-1(riga, 4), cell-data col-qta
                              modify form1-gd-1(riga, 5), cell-data col-prz
                              add 1 to riga
                           end-perform
                     end-start    
                     perform varying idx from 1 by 1 
                               until idx > 5
                        add 1 to idx giving riga
                        modify gd-note(riga, 1), cell-data tco-nota(idx)
                     end-perform
                end-read
           end-evaluate.

           perform CALCOLA-TOTALE.
           move 78-ID-ef-data to control-id.
           move 4             to accept-control.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterBufToFld
BEGIN-TEXT
           evaluate true
           when vdocum-tipo-fattura
                move ef-anno-buf   to tno-fattura-anno
                move ef-numero-buf to tno-fattura-numero 
                
           when vdocum-tipo-corrisp
                move ef-anno-buf   to tno-corrisp-anno
                move ef-numero-buf to tno-corrisp-numero
           end-evaluate.

           move ef-cliente-buf    to tno-cliente.
           move ef-data-buf       to como-data.
           perform DATE-TO-FILE
           move como-data         to tno-data-doc.
           move cbo-pagamento-buf to tno-pagamento.
           move cbo-iban-buf      to tno-iban.
           
           move importo-sub       to tno-importo-sub.   
           move importo-iva       to tno-importo-iva.
           move importo-tot       to tno-importo-tot.   
           move save-iva          to tno-iva.

           accept tno-data-creazione from century-date.

           perform varying riga from 2 by 1
                     until riga > 6
              subtract 1 from riga giving idx
              inquire gd-note(riga, 1), cell-data in tno-nota(idx)
           end-perform
END-TEXT
END-DATA
