           move como-numero       to tno-numero.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           set tutto-ok to true.
           inquire form1-gd-1, cursor-y in riga,
                               cursor-x in colonna,
                               last-row in tot-righe.
      
           if event-data-2 not = riga
      
              perform VALORE-RIGA

              if not RigaVuota
                 perform varying colonna from 1 by 1
                           until colonna > 5
                    perform CONTROLLO-RIGA
                    if errori exit perform end-if
                 end-perform
              end-if

              if tutto-ok
                 evaluate true
                 when event-data-2 < 2 
                      move 2            to riga
                      move event-data-1 to colonna
      
                 when event-data-2 > tot-righe
                      add  1                to tot-righe giving riga
                      set event-action      to event-action-fail
                      move 1                to colonna
                      perform PUNTA-CELLA
                 when other
                      move event-data-2 to riga
                      move event-data-1 to colonna
                 end-evaluate
                 perform VALORE-RIGA
                 if art-codice = 0
                    initialize rno-rec 
                               replacing numeric data by zeroes
                                    alphanumeric data by spaces
                 end-if
                 move rno-rec       to old-rno-rec
              else                  
                 set event-action   to event-action-fail
                 perform PUNTA-CELLA
              end-if
           else
              if event-data-1 not = colonna
                 inquire Form1-Gd-1(riga, 1), cell-data in art-codice
                 if art-codice = 0
                    set event-action to event-action-fail
                    move 1 to colonna event-data-1
                    perform PUNTA-CELLA
                 end-if
              end-if
              move event-data-1 to colonna
           end-if.
      
           perform COLORE-RIGA.
           move BitmapZoomDisabled to BitmapNumZoom.
           move 0 to e-cerca.

           if colonna = 1
              move BitmapZoomEnabled to BitmapNumZoom
              move 1 to e-cerca
           end-if.
           
           modify tool-cerca, enabled e-cerca, 
                              bitmap-number BitmapNumZoom.


      ***---
       COLORE-RIGA.
           modify  form1-gd-1, start-y = event-data-2, 
                                     y = event-data-2,
                               start-x = 1, 
                                     x = 5,
                          region-color = 353.


      ***---
       PUNTA-CELLA.
           move 78-ID-Form1-Gd-1 to control-id store-id.
           move 4                to accept-control.
           modify Form1-Gd-1,    cursor-y riga, 
                                 cursor-x colonna.
           move riga to event-data-2.
           perform COLORE-RIGA.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           set SiSalvato to true.
           perform Form1-BUF-TO-FLD.
           move 0 to scelta.

           display message "Abbandonare la creazione del documento?"
                     title titolo
                      type mb-yes-no
                    giving scelta       
      
           evaluate scelta
           when mb-yes 
                continue
           when mb-no
                perform CANCELLA-COLORE
                set errori to true
                move store-id to CONTROL-ID       
                move 4        to ACCEPT-CONTROL   
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           set tutto-ok to true.
           perform varying control-id from 78-ID-ef-data by 1 
                     until control-id > 78-ID-ef-cliente
              perform CONTROLLO
              if errori
                 exit perform
              end-if
           end-perform.

           if tutto-ok
              inquire ef-data, value in ef-data-buf
              move ef-data-buf to como-data
              perform DATE-TO-FILE
              move como-data(1:4) to como-anno
              move como-anno   to tno-anno
              move como-numero to tno-numero
              read tnotacr no lock
                   invalid continue
               not invalid
                   set errori to true
                   move tno-data-doc to como-data
                   perform DATE-TO-SCREEN
                   move como-data to data-doc
                   display message "ESISTE GIA' LA NOTA CREDITO N. " tno-numero
                            x"0d0a""CON DATA " data-doc
                            x"0d0a""CONTROLLARE I CONTATORI NEI PARAMETRI DITTA"
                             title tit-err
                              icon 2
              end-read
           end-if.
      
           if tutto-ok                        
              move low-value to tno-rec
              move como-data to tno-data-doc
              start tnotacr key >= tno-k-data
                    invalid continue
                not invalid
                    perform until 1 = 2
                       read tnotacr next at end exit perform end-read
                       if tno-anno not = como-anno
                          exit perform 
                       end-if
                       if tno-numero < como-numero
                          if tno-data-doc > como-data
                             set errori to true
                          end-if
                       end-if
                       if tno-numero > como-numero
                          if tno-data-doc < como-data
                             set errori to true
                          end-if
                       end-if
                       if errori
                          move 78-ID-ef-data to store-id control-id
                          move tno-data-doc to como-data
                          perform DATE-TO-SCREEN
                          move como-data to data-doc
                          display message "ESISTE NOTA CREDITO N. " tno-numero
                                   x"0d0a""CON DATA " data-doc
                                    title tit-err
                                     icon 2
                          exit perform
                       end-if
                    end-perform
              end-start
           end-if.

           if tutto-ok
              if importo-sub = 0
                 set errori to true
                 move 78-ID-form1-gd-1 to control-id
                 display message "IMPOSSIBILE REGISTRARE DOCUMENTO A ZERO"
                            title tit-err
                             icon 2
              end-if
           end-if.

           if tutto-ok
              move 78-ID-form1-gd-1 to control-id
              inquire form1-gd-1, cursor-y in riga
              perform VALORE-RIGA
              perform varying colonna from 1 by 1 
                        until colonna > 4
                 perform CONTROLLO-RIGA
                 if errori
                    exit perform
                 end-if
              end-perform
           end-if.
      
           if errori
              perform CANCELLA-COLORE
              move 4 to ACCEPT-CONTROL
           else
              perform FORM1-BUF-TO-FLD
              perform CANCELLA-COLORE 

              close    param
              open i-o param
              move spaces to prm-chiave
              read param  no lock
              move como-numero to prm-notecr-num
              if tno-data-doc > prm-notecr-data
                 move tno-data-doc to prm-notecr-data
              end-if
              rewrite prm-rec
              close      param 
              open input param

              move importo-sub to tno-importo-sub
              move importo-iva to tno-importo-iva
              move importo-tot to tno-importo-tot

              write tno-rec       

              inquire form1-gd-1, last-row in tot-righe
              perform varying riga from 2 by 1 
                        until riga > tot-righe
                 move tno-chiave to rno-chiave-testa
                 perform VALORE-RIGA
                 subtract 1 from riga giving rno-riga
                 move col-art     to rno-articolo
                 move col-des     to rno-descrizione
                 move col-listino to rno-listino
                 move col-qta     to rno-qta
                 move col-prz     to rno-prz
                 move tno-iva     to rno-iva
                 write rno-rec
              end-perform

              display message "CREATA NOTA CREDITO N. " prm-notecr-num
                        title titolo

              move spaces to link-stampante

              call   "st-docum" using tno-chiave, "N", link-stampante
              cancel "st-docum"

              set vecchio to true       
              move 27 to key-status
           end-if.   
                 
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-RIGA
BEGIN-TEXT
           inquire form1-gd-1(riga, 1), cell-data in col-art.
           inquire form1-gd-1(riga, 2), cell-data in col-des.
           inquire form1-gd-1(riga, 3), cell-data in col-listino.
           inquire form1-gd-1(riga, 4), cell-data in col-qta.
           inquire form1-gd-1(riga, 5), cell-data in col-prz.

           move col-qta to como-qta.
           move col-prz to como-prz.

           if col-art      = spaces and
              col-des      = spaces and
              como-qta     = 0 and
              como-prz     = 0
              set RigaVuota to true
           else
              set RigaVuota to false
           end-if.
