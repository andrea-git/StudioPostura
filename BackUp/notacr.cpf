BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = Form1-Gd-1-Ev-Msg-Goto-Cell-Mouse
BEGIN-TEXT
           perform SPOSTAMENTO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = Form1-Gd-1-Ev-Msg-Finish-Entry
BEGIN-TEXT
           set tutto-ok to true.
           inquire form1-gd-1, cursor-y in riga, last-row in tot-righe.
           perform VALORE-RIGA.
      
           evaluate colonna
           when 1
                if col-art = spaces
                   if not RigaVuota
                      display message "Cancellare la riga?"
                                title titolo
                                 type mb-yes-no
                              default mb-no
                               giving scelta
                      if scelta = mb-no
                         set errori to true
                      else
                         initialize rec-grid 
                                    replacing numeric data by zeroes
                                         alphanumeric data by spaces
      
                         move riga to store-riga
                         add 1 to riga
                         perform VALORE-RIGA
                         move rno-rec    to old-rno-rec
                         move store-riga to riga
                 
                         modify form1-gd-1, record-to-delete = riga
                         modify form1-gd-1, insert-rows = 1
                      end-if
                   end-if
                else
                   perform CONTROLLO-RIGA
                end-if
                if tutto-ok
                   if rno-articolo not = old-rno-articolo
                      move riga to RigaCambiata
                      move 1    to ColonnaCambiata
                   end-if
                end-if
      
           when 2
                perform CONTROLLO-RIGA
                if tutto-ok
                   if col-des not = old-col-des
                      move riga to RigaCambiata
                      move 2    to ColonnaCambiata
                   end-if
                end-if
      
           when 4
                perform CONTROLLO-RIGA
                if col-qta not = old-col-qta
                   move riga  to RigaCambiata
                   move 3     to ColonnaCambiata
                end-if

           when 5
                perform CONTROLLO-RIGA
                if col-prz not = old-col-prz
                   move riga  to RigaCambiata
                   move 4     to ColonnaCambiata
                end-if 
           end-evaluate.
      
           if errori
              move 78-ID-form1-gd-1 to store-id
              set event-action to event-action-fail
           else                                              
              modify form1-gd-1(riga, 1),   cell-data col-art
              modify form1-gd-1(riga, 2),   cell-data col-des
              modify form1-gd-1(riga, 3),   cell-data col-listino
              modify form1-gd-1(riga, 4),   cell-data col-qta
              modify form1-gd-1(riga, 5),   cell-data col-prz
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = Form1-Gd-1-Ev-Msg-Begin-Entry
BEGIN-TEXT
           evaluate colonna 
           when 3
                set event-action to event-action-fail
           when 5
                set environment "KEYSTROKE" to "DATA=44 46"  
           end-evaluate.
END-TEXT
END-DATA

@}}

