BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = ttipocli
   ACTOBJ            = ttipocli
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       codtcl-ERR SECTION.
           use after error procedure on tipocli.
           evaluate status-tipocli
           when "35"
                display message "File [tipocli] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [tipocli] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[tipocli] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true 
           when "93"             
                move 0 to mod
                modify tool-modifica, value = mod  
                set StatusVisua to true
                open input tipocli
                display message box MSG-Gia-in-uso-su-altro-terminale
                            x"0d0a" MSG-SOLA-VISUA
                        title = tit-err
                        icon is mb-error-icon      
           end-evaluate.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterAccept
BEGIN-TEXT
      *    per il bottone modifica
           if key-status = 5
              inquire tool-modifica, enabled in e-modifica
              if e-modifica = 1
                 if mod = zero
                    move 1    to mod
                 else 
                    move zero to mod
                 end-if
                 modify tool-modifica, value = mod
                 perform MODIFICA
              end-if
           end-if.
END-TEXT
END-DATA
