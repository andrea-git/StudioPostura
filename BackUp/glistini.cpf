BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ABILITAZIONI
BEGIN-TEXT
           if mod = 1              
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else           
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move 0 to e-salva e-cancella
           end-if.      


           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-nuovo, tool-salva, tool-cancella, tool-modifica.
      * FINE.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ANTEPRIMA
BEGIN-TEXT
           set s-anteprima to true.
           perform STAMPA-ANTEPRIMA.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.
      *
           display message box MSG-Cancellare-il-record-corrente
                   title   titolo
                   type    mb-yes-no 
                   default mb-no
                   giving  scelta

           if scelta = mb-yes
              perform RIEMPI-CHIAVE
              delete tlistini record 
                     invalid  continue
                 not invalid  perform DELETE-RIGHE
              end-delete

              move 0   to mod
              move 1   to mod-k
              perform FORM1-CLEAR
              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC
              perform RESETTA-GRID
              
              display message box MSG-Cancellazione-avvenuta-con-successo
                      title titolo                         

              unlock tlistini all records

              perform PRIMO 
              move 78-ID-ef-codice to control-id     
              move 4               to accept-control 

              set Statusvisua to true
              perform STATUS-BAR-MSG
              modify TOOL-MODIFICA value = mod
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate CONTROL-ID
           when 78-ID-ef-tipo
                if Form1-RADIO-1-BUF = 1
                   inquire ef-tipo, value in tcl-codice
                   move "tipocli"      to Como-File
                   call   "zoom-gt" using como-file, tcl-rec
                                   giving stato-zoom
                   cancel "zoom-gt"
                   if stato-zoom = 0
                      move tcl-codice      to ef-tipo-buf
                      move tcl-descrizione to lab-tipo-buf
                      display ef-tipo lab-tipo
                      move 4 to accept-control
                   end-if
                else
                   inquire ef-tipo, value in cli-codice
                   move "clienti"      to Como-File
                   call   "zoom-gt" using como-file, cli-rec
                                   giving stato-zoom
                   cancel "zoom-gt"
                   if stato-zoom = 0
                      move cli-codice      to ef-tipo-buf
                      perform DESCRIZIONE-CLIENTE
                      display ef-tipo lab-tipo
                      move 4 to accept-control
                   end-if
                end-if
      *
           when 78-ID-gd1
                perform X-Y
                evaluate colonna
                when 78-col-art
                     inquire gd1(riga, 78-col-art), 
                                               cell-data in art-codice
                     move "articoli"     to Como-File
                     call   "zoom-gt" using  como-file, art-rec
                                      giving stato-zoom
                     cancel "zoom-gt"
                     if stato-zoom = 0
                        perform CONTROLLA-ARTICOLO-GIA-INSERITO
                        if not trovato
                           move art-codice        to col-art
                           move art-descrizione   to col-des
                           modify gd1(riga, 78-col-art), 
                                                     cell-data = col-art
                           modify gd1(riga, 78-col-des), 
                                                     cell-data = col-des
                           perform CONTROLLO-RIGA
                        end-if
                     end-if
                end-evaluate
           end-evaluate. 

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = COLORE-RIGA
BEGIN-TEXT
           modify gd1, start-x = 1,
                             x = 78-max-colonne,
                       start-y = riga,
                             y = riga,
                  region-color = 257,
                  cursor-color = colore-nu.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CLEAR-SCREEN
BEGIN-TEXT
           move tls-chiave to old-tls-chiave.

           initialize tls-dati of tlistini 
                     replacing numeric data by zeroes
                          alphanumeric data by spaces.
                         
           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.

           display form1. 

           perform RESETTA-GRID.
  
           unlock tlistini all records.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-INSERIMENTO
BEGIN-TEXT
      * MODIFICA LA STATUS-BAR
      * CONTROLLO SE DA INSERIMENTO SCORRO. ALLORA DEVO SEGNALARE CHE
      * SONO TORNATO IN MODIFICA.
           inquire tool-modifica, value in mod.

           evaluate mod
           when 1  
                set StatusModifica to true
           when 0
                set StatusVisua    to true   
           end-evaluate.
      
           perform STATUS-BAR-MSG.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLA-ARTICOLO-GIA-INSERITO
BEGIN-TEXT
           set trovato  to false.

           perform varying store-riga from 2 by 1 until
                           store-riga > tot-righe
              inquire gd1(store-riga, 1), cell-data in store-articolo
              if store-articolo  = art-codice and 
                 store-riga  not = riga
                 set trovato to true
                 subtract 1 from store-riga
                 display message "Articolo già inserito a riga: " store-riga
                           title tit-err
                            icon 2
                 set errori to true
                 move spaces to col-des
                 modify gd1(riga, 2), cell-data = col-des
                 exit perform
              end-if
           end-perform.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true. 
      
           evaluate CONTROL-ID
           when 78-ID-ef-codice
                perform RIEMPI-CHIAVE           
                if ef-codice-buf not = 0
                   read tlistini no lock
                        invalid
                        if vecchio
                           display message MSG-record-inesistente
                                     title tit-err
                                      icon 2
                           perform CLEAR-SCREEN
                           set errori to true
                        end-if
                    not invalid      
                        if old-tls-chiave not = tls-chiave or
                         ( tls-codice = 0 and mod = 0 )
                           perform CURRENT-RECORD 
                        end-if
                   end-read
                   move 78-ID-ef-codice to control-id
                   move 4               to accept-control
      
                else
                   set errori to true  
                   move 78-ID-ef-codice to control-id
                   display message box MSG-record-inesistente
                           title = tit-err
                           icon mb-warning-icon
                end-if

           when 78-ID-ef-descrizione
                inquire ef-descrizione, value in ef-descrizione-buf
                if ef-descrizione-buf = spaces
                   set errori to true
                   display message "Descrizione obbligatoria"
                             title tit-err
                              icon 2
                end-if
      
           when 78-ID-ef-ini-val
                inquire ef-ini-val, value in como-data
                perform DATE-FORMAT
                move como-data to ef-ini-val-buf
                display ef-ini-val
      
           when 78-ID-ef-tipo
                if form1-radio-1-buf = 1
                   inquire ef-tipo, value in tcl-codice
                   read tipocli no lock 
                        invalid
                        set errori to true
                        move spaces to tcl-descrizione
                        display message "Codice non valido"
                                  title tit-err
                                   icon 2
                   end-read
                   move tcl-descrizione to lab-tipo-buf
                else
                   inquire ef-tipo, value in cli-codice
                   read clienti no lock 
                        invalid
                        set errori to true
                        move spaces to cli-ragsoc cli-nome
                        display message "Codice non valido"
                                  title tit-err
                                   icon 2
                   end-read
                   perform DESCRIZIONE-CLIENTE
                end-if
                display lab-tipo
      
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-RIGA
BEGIN-TEXT
           set tutto-ok to true.
           if mod = 0 
              exit paragraph 
           end-if.
      
           evaluate colonna
           when 78-col-art
                move col-art to art-codice
                read articoli no lock
                     invalid
                     set errori to true
                     display message "Articolo NON valido"
                               title tit-err
                                icon 2
                     move spaces to art-descrizione
                end-read
                modify gd1(riga, 78-col-des), cell-data art-descrizione
      
           end-evaluate.                         

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if nuovo move 0 to mod end-if.
      
           if mod = 1
              read tlistini lock 
                 invalid
                    set errori to true 
              end-read
           else
              read tlistini no lock 
                 invalid 
                    set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.
      
           if RecLocked
              set RecLocked to false
           else
              if tutto-ok
                 perform CANCELLA-COLORE
                 if nuovo
                    move 0      to mod
                    move 1      to mod-k
                    move 78-ID-ef-codice to control-id
                    move 4      to accept-control
                 else
                    move 78-ID-ef-descrizione to control-id
                    move 4      to accept-control
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else                         
                    set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG 
                 perform VALORIZZA-OLD
                 perform RIEMPI-GRID
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-codice to CONTROL-ID
                       move 4               to accept-control
                       display message MSG-Record-inesistente
                                 title tit-err
                                  icon 2
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = DELETE-RIGHE
BEGIN-TEXT
           move tls-codice to rls-codice.
           move low-value  to rls-articolo.
           start rlistini key >= rls-chiave
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read rlistini next at end exit perform end-read
                    if rls-codice not = tls-codice
                       exit perform 
                    end-if
                    delete rlistini record invalid continue end-delete
                 end-perform
           end-start.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = DESCRIZIONE-CLIENTE
BEGIN-TEXT
           initialize lab-tipo-buf.
           inspect cli-ragsoc replacing trailing spaces by low-value.
           string  cli-ragsoc delimited low-value
                   " "        delimited size
                   cli-nome   delimited size
                   into lab-tipo-buf
           end-string.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT
BEGIN-TEXT
           move 0 to StatusHelp.
           move 1 to mod-k.
           set StatusVisua    to true. 
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-tls-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.

           initialize old-rls-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1   to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    move 0   to mod-k
                    move 1   to mod
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1 to mod
                 perform SALV-MOD            
                 move 0 to mod
                 move 1 to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    move 78-ID-ef-codice to control-id
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock tlistini all records
                 end-if
              end-if

              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           perform SALV-MOD.

           if tutto-ok  
              move 78-ID-ef-descrizione to control-id       
              move 4               to accept-control   
              move 1               to mod mod-k
              modify tool-modifica,  value = mod
              perform CANCELLA-COLORE                  
              perform VALORIZZA-NUOVO

              perform INIT-OLD-REC
              move tls-chiave to old-tls-chiave
              set StatusIns to true
              perform STATUS-BAR-MSG  
              set nuovo to true
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRECEDENTE
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock tlistini all records 
      
              perform RIEMPI-CHIAVE
      
              start tlistini key < tls-chiave
                    invalid  set errori to true
                not invalid 
                    read tlistini previous no lock
                       at end set errori to true 
                    end-read
              end-start

              if tutto-ok
                 if nuovo
                    perform CANCELLA-COLORE
                    move 78-ID-ef-codice to control-id
                    move 4               to accept-control
                 end-if
                 move tls-codice         to ef-codice-buf
                 display ef-codice
                 set ReadSecca           to false
                 perform CURRENT-RECORD
              else
                 display message "File [TLISTINI]"
                          x"0d0a""Raggiunto inizio file"
                           title titolo
              end-if
      
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRIMO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock tlistini all records 
      
              move low-value   to tls-chiave of tlistini
              start tlistini key >= tls-chiave of tlistini
                 invalid  
                    set errori to true
                 not invalid 
                    read tlistini next no lock
                       at end
                          set errori to true 
                    end-read
              end-start
      
              if tutto-ok
                 if nuovo
                    perform CANCELLA-COLORE
                    move 78-ID-ef-codice to control-id
                    move 4               to accept-control
                 end-if
                 move tls-codice of tlistini to ef-codice-buf
                 display ef-codice
                 set  ReadSecca            to false
                 perform CURRENT-RECORD
              else
                 perform CLEAR-SCREEN
                 display message "File [TLISTINI]"
                          x"0d0a""Archivio vuoto"
                           title titolo
                 perform CANCELLA-COLORE
                 move 78-ID-ef-codice to control-id
                 move 4               to accept-control
              end-if
      
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PUNTA-CELLA
BEGIN-TEXT
           move 78-ID-gd1    to control-id store-id.
           move 4            to accept-control.
           modify gd1,       cursor-y riga, 
                             cursor-x colonna.
           perform COLORE-RIGA.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RESETTA-GRID
BEGIN-TEXT
           modify gd1, reset-grid = 1.
           perform GD1-CONTENT.
           initialize rec-grid.
           modify gd1, insert-rows = 999.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           inquire ef-codice, value in ef-codice-buf.
           move ef-codice-buf to tls-codice.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-GRID
BEGIN-TEXT
           modify gd1, mass-update = 1.
           perform RESETTA-GRID.
           move tls-codice to rls-codice.
      
           set tutto-ok   to true
           move low-value to rls-articolo
           start rlistini key >= rls-chiave
                 invalid  set errori  to true
           end-start.
      
           if tutto-ok
              move 2 to riga
              perform until 1 = 2
                 read rlistini next no lock 
                    at end 
                       exit perform 
                 end-read
                 if rls-codice not = tls-codice
                    exit perform
                 end-if
                 move rls-articolo to art-codice col-art
                 read articoli no lock 
                      invalid move "** NON TROVATO **" to art-descrizione
                 end-read
                 move art-descrizione        to col-des
                 move rls-prezzo1            to col-prz1
                 move rls-prezzo2            to col-prz2
                 move rls-prezzo3            to col-prz3
      
                 modify gd1(riga, 78-col-art),  cell-data col-art       
                 modify gd1(riga, 78-col-des),  cell-data col-des       
                 modify gd1(riga, 78-col-prz1), cell-data col-prz1      
                 modify gd1(riga, 78-col-prz2), cell-data col-prz2      
                 modify gd1(riga, 78-col-prz3), cell-data col-prz3
                 add 1 to riga
              end-perform
              subtract 2 from riga giving RigheIniziali
           end-if.
           modify gd1, mass-update = 0.
           move 2 to riga.
           move 1 to colonna.
           perform PUNTA-CELLA.
           perform VALORE-RIGA.
           move rls-rec      to old-rls-rec.
           move rls-articolo to como-old-rls-articolo

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = 0 
              exit paragraph 
           end-if.

           set tutto-ok to true.

           set controllo-finale to true.
           perform varying control-id from 78-ID-ef-codice by 1
                     until control-id > 78-ID-ef-tipo
              perform CONTROLLO
              if errori 
                 exit perform
              end-if
           end-perform.
      
           if tutto-ok
              perform X-Y
              if tot-righe = 1
                 set errori to true
                 display message "Nessun articolo inserito"
                           title tit-err
                            icon 2
                 move 78-ID-gd1 to control-id
                 move 1 to colonna
              else
                 perform DELETE-RIGHE
                 perform FORM1-BUF-TO-FLD
                 perform varying riga from 2 by 1 
                           until riga > tot-righe
                    perform VALORE-RIGA
                    perform SCRIVI-RIGA
                 end-perform
              end-if
           end-if.

           set controllo-finale to false
      
           if errori
              perform ABILITAZIONI
              if store-id = 78-ID-gd1
                 perform CANCELLA-COLORE
                 perform PUNTA-CELLA
              else
                 move store-id to control-id
                 move 4        to accept-control
              end-if
           else

              perform FORM1-BUF-TO-FLD
              perform CANCELLA-COLORE

              write tls-rec invalid rewrite tls-rec end-write 

              set vecchio to true

              set vecchio      to true
              set stampa       to true

              perform TORNA-IN-VISUA
              perform VALORIZZA-OLD

           end-if.

           display form1.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD.

           if SiSalvato and mod = 1
              if RigaCambiata not = 0 or ColonnaCambiata not = 0
                 set NoSalvato  to true
                 move 78-ID-gd1 to store-id
                 move ColonnaCambiata to colonna
                 move RigaCambiata    to riga
              end-if
           end-if.    

           if SiSalvato and mod = 1
              inquire gd1, last-row in tot-righe
              subtract 1 from tot-righe
              if tot-righe not = RigheIniziali |or inserito-extra
                 add 2 to RigheIniziali giving riga
                 set NoSalvato  to true
                 move 78-ID-gd1 to store-id
                 move 1 to colonna
              end-if
           end-if.

           |Specifico per il programma dei promo
           if SiSalvato and mod = 1
              perform X-Y
              |non lascio una testa di un volantino senza righe 
              if tot-righe = 1
                 perform RIEMPI-CHIAVE
                 delete tlistini record invalid continue end-delete
              end-if
           end-if.
           ||||
      
           if NoSalvato
              display message MSG-Salvare-le-modifiche
                        title titolo
                         type mb-yes-no-cancel 
                       giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
                   if nuovo
                      perform RIEMPI-CHIAVE
                      delete tlistini record invalid continue end-delete
                   end-if
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   evaluate store-id
                   when 78-ID-gd1     
                        perform PUNTA-CELLA
                   when other
                      move store-id to control-id       
                      move 4        to accept-control   
                   end-evaluate
              end-evaluate
           else
              if nuovo
                 perform RIEMPI-CHIAVE
                 delete tlistini record invalid continue end-delete
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SCRIVI-RIGA
BEGIN-TEXT
           if rls-articolo = spaces
              exit paragraph
           end-if.

           move tls-codice  to rls-codice.
           move tls-ricerca to rls-ricerca.
           move col-art     to rls-articolo.
           move col-prz1    to rls-prezzo1.
           move col-prz2    to rls-prezzo2.
           move col-prz3    to rls-prezzo3.

           write rls-rec invalid rewrite rls-rec end-write.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA
BEGIN-TEXT
      *****     set tutto-ok to true.
      *****     move zero   to link-fornitore
      *****     perform SEL-LISTINI.
      *****
      ********---
      ***** SEL-LISTINI.
      *****     call   "sel-listini" using link-codice 
      *****                                link-articolo 
      *****                                link-fornitore.
      *****     cancel "sel-listini".
      *****
      *****     if link-codice not = 0
      *****
      *****        if link-codice not = tls-codice of tlistini
      *****           perform SALV-MOD
      *****        end-if
      *****
      *****        if tutto-ok
      *****           set  ReadSecca   to true
      *****           move link-codice to tls-codice of tlistini 
      *****           move link-codice to ef-codice-buf
      *****           display ef-codice
      *****           perform CURRENT-RECORD
      *****           if link-articolo = 0
      *****              move 2 to riga
      *****           else
      *****              move store-riga to riga
      *****           end-if
      *****           move 1 to colonna
      *****           perform PUNTA-CELLA
      *****           perform CANCELLA-COLORE
      *****           if link-articolo = 0
      *****              move 78-ID-ef-codice to control-id
      *****              move 4               to accept-control
      *****           end-if
      *****        end-if
      *****
      *****     end-if.
      *****
      *****
      ********---
      ***** SELEZIONA-NUMERICO.
      *****     if tls-codice of tlistini not > 0
      *****        set errori to true
      *****        move 78-ID-ef-codice to control-id
      *****        display message box msg-codice-obbligatorio
      *****                title tit-err
      *****                icon  mb-warning-icon
      *****     else           
      *****        read tlistini no lock
      *****             invalid
      *****             if vecchio
      *****                display message MSG-record-inesistente
      *****                          title tit-err
      *****                           icon 2
      *****                perform CLEAR-SCREEN
      *****                set errori to true
      *****             end-if
      *****         not invalid      
      *****             if old-tls-chiave not = tls-chiave of tlistini or
      *****              ( tls-codice of tlistini = 0 and mod = 0 )
      *****                perform CURRENT-RECORD 
      *****             end-if
      *****        end-read
      *****     end-if.
      *****
      ********---
      ***** SELEZIONA-ALFA.
      ****** LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      ****** ANCORA GLI ZERI DAVANTI CHE ELIMINO
      *****     inspect ef-codice-buf replacing leading ZERO by SPACES.
      *****     move ef-codice-buf to cli-ragsoc-1.
      *****
      *****
      ****** POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      ****** ALTERNATA
      *****     call "C$JUSTIFY" using cli-ragsoc-1, "L".
      *****     move low-value to cli-codice
      *****                  
      *****     set cli-tipo-F to true.
      *****     start clienti key not < cli-k1
      *****        invalid 
      *****           initialize cli-rec
      *****        not invalid 
      *****           read clienti next 
      *****              if cli-tipo-C
      *****                 initialize cli-rec
      *****              end-if
      *****     end-start.
      *****     set cli-tipo-F to true.
      *****
      *****     if cli-ragsoc-1 = ef-codice-buf
      *****        move cli-codice   to link-fornitore
      *****        perform SEL-LISTINI
      *****     else
      *****        move "clienti-alfa-all" to como-file
      *****        call "zoom-gt"  using  como-file, cli-rec
      *****                        giving  stato-zoom
      *****        cancel "zoom-gt"
      *****        if stato-zoom = 0
      *****           move cli-codice    to link-fornitore
      *****           perform SEL-LISTINI
      *****        end-if
      ******     else
      ******        set errori           to true
      ******        move 78-ID-ef-codice to CONTROL-ID
      *****     end-if.
      *****

           inquire ef-codice, value in tls-codice.
           move "tlistini"     to Como-File
           call   "zoom-gt" using  como-file, tls-rec
                            giving stato-zoom
           cancel "zoom-gt"
           if stato-zoom = 0
              if old-tls-chiave not = tls-chiave or
               ( tls-codice = 0 and mod = 0 )
                 move tls-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to cli-chiave
                    set ReadSecca to true
                    modify ef-codice, value tls-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-descrizione to CONTROL-ID       
                    move 4                    to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if.
       
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           set tutto-ok to true.
           perform X-Y.
      
           if event-data-2 not = riga
      
              perform VALORE-RIGA

              if not RigaVuota
                 perform varying colonna from 78-col-art by 1
                           until colonna > 78-max-colonne|78-col-tipo-imp
                    perform CONTROLLO-RIGA
                    if errori exit perform end-if
                 end-perform
              end-if
      
              if tutto-ok and mod = 1
                 perform SCRIVI-RIGA
              end-if

              if tutto-ok
                 evaluate true
                 when event-data-2 < 2 
                      move 2            to riga
                      move event-data-1 to colonna
      
                 when event-data-2 > tot-righe
                      add  1                to tot-righe giving riga
                      set event-action      to event-action-fail
                      move 1                to colonna
                      perform PUNTA-CELLA
                 when other
                      move event-data-2 to riga
                      move event-data-1 to colonna
                 end-evaluate
                 perform VALORE-RIGA
                 if art-codice = 0
                    initialize rls-rec 
                               replacing numeric data by zeroes
                                    alphanumeric data by spaces
                 end-if
                 move rls-rec to old-rls-rec
                 move rls-articolo   to como-old-rls-articolo
              else
                 set event-action    to event-action-fail
                 perform PUNTA-CELLA
              end-if
           else
              if event-data-1 not = colonna
                 inquire gd1(riga, 1), cell-data in art-codice
                 if art-codice = 0
                    set event-action to event-action-fail
                    move 1 to colonna event-data-1
                    perform PUNTA-CELLA
                 end-if
              end-if
              move event-data-1 to colonna
           end-if.
      
           perform COLORE-RIGA.
           move 0 to StatusHelp.
           if mod = 1 and colonna = 78-col-art
              move 1 to StatusHelp
           end-if.
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA
BEGIN-TEXT
           set s-stampa to true.
           perform STAMPA-ANTEPRIMA.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA-ANTEPRIMA
BEGIN-TEXT
      *****     if nuovo
      *****        display message "Occorre salvare il volantino"
      *****                        " per poterlo stampare"
      *****                  title titolo
      *****                   icon 2
      *****     else
      *****        inquire ef-codice, value in tls-codice of tlistini
      *****        read tlistini no lock
      *****              invalid
      *****              display message "Volantino NON valido"
      *****                      title tit-err
      *****                      icon 2
      *****          not invalid
      *****              call "W$MOUSE" using set-mouse-shape, wait-pointer
      *****
      *****              move tls-chiave of tlistini  to stls-lis-chiave
      *****
      *****              evaluate true
      *****              when s-stampa
      *****                   set stls-printer  to true
      *****              when s-excell
      *****                   set stls-excell   to true
      *****              when other
      *****                   set stls-anteprima   to true
      *****              end-evaluate
      *****              move user-codi to stls-user
      *****     
      *****              call   "stlistini" using stlistini-linkage
      *****              cancel "stlistini"
      *****
      *****
      ******              evaluate true
      ******              when s-excell
      ******                   set stls-excell   to true
      ******                   call   "stlistini" using stlistini-linkage
      ******                   cancel "stlistini"
      ******              when other
      ******                   display message "In attesa di layout"
      ******                           title titolo
      ******                           icon 2
      ******              end-evaluate
      *****     
      *****             call "W$MOUSE" using set-mouse-shape, arrow-pointer
      *****        end-read
      *****     end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SUCCESSIVO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock tlistini all records 
      
              perform RIEMPI-CHIAVE
      
              start tlistini key > tls-chiave of tlistini
                 invalid  
                    set errori to true
                 not invalid 
                    read tlistini next no lock
                       at end   
                          set errori to true 
                    end-read
              end-start
      
              if tutto-ok
                 if nuovo
                    perform CANCELLA-COLORE
                    move 78-ID-ef-codice to control-id
                    move 4               to accept-control
                 end-if
                 move tls-codice of tlistini to ef-codice-buf
                 display ef-codice
                 set ReadSecca             to false
                 perform CURRENT-RECORD
              else
                 display message "File [TLISTINI]"
                          x"0d0a""Raggiunta fine file"
                           title titolo
              end-if
      
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-codice to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock tlistini all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ULTIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok                 
              perform CHECK-INSERIMENTO
              unlock tlistini all records 
      
              move high-value to tls-chiave of tlistini
              start tlistini key < tls-chiave of tlistini
                 invalid  
                    set errori to true
                 not invalid 
                    read tlistini previous no lock
                       at end   
                          set errori to true 
                    end-read
              end-start
      
              if tutto-ok
                 if nuovo
                    perform CANCELLA-COLORE
                    move 78-ID-ef-codice to control-id
                    move 4               to accept-control
                 end-if
                 move tls-codice of tlistini to ef-codice-buf
                 display ef-codice
                 set  ReadSecca            to false
                 perform CURRENT-RECORD
              else
                 perform CLEAR-SCREEN
                 display message "File [TLISTINI]"
                          x"0d0a""Archivio vuoto"
                           title titolo
                 perform CANCELLA-COLORE
                 move 78-ID-ef-codice to control-id
                 move 4               to accept-control
              end-if
      
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-RIGA
BEGIN-TEXT
           inquire gd1(riga, 78-col-art),   cell-data col-art.
           inquire gd1(riga, 78-col-des),   cell-data col-des.
           inquire gd1(riga, 78-col-prz1),  cell-data col-prz1.
           inquire gd1(riga, 78-col-prz2),  cell-data col-prz2.
           inquire gd1(riga, 78-col-prz3),  cell-data col-prz3.

           move col-prz1 to como-prz1.
           move col-prz2 to como-prz2.
           move col-prz3 to como-prz3.

           if col-art   = spaces and
              col-des   = spaces and
              como-prz1 = 0 and
              como-prz2 = 0 and
              como-prz3 = 0
              set RigaVuota to true
           else
              set RigaVuota to false
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move tls-rec to old-tls-rec.

           set vecchio to true.

           move 0 to StatusHelp
           perform STATUS-HELP.

           inquire gd1, last-row in tot-righe.
           subtract 1 from tot-righe giving RigheIniziali.
           move 0 to RigaCambiata ColonnaCambiata.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-NUOVO
BEGIN-TEXT
           move high-value to tls-codice of tlistini.

           start tlistini key <=   tls-codice of tlistini
                 invalid move 1 to tls-codice of tlistini
             not invalid
                 read tlistini previous with no lock
                       at end move 1 to tls-codice of tlistini
                   not at end add  1 to tls-codice of tlistini
                 end-read
           end-start.
           
           move "00"    to status-tlistini.

           initialize tls-dati of tlistini 
                      replacing numeric data by zeroes
                           alphanumeric data by spaces.

           initialize rls-rec replacing numeric data by zeroes
                                    alphanumeric data by spaces.

           write tls-rec invalid rewrite tls-rec end-write.
           read tlistini lock invalid continue end-read.
           
           set tls-tipo-categoria     to true.
           set OLD-tls-tipo-categoria to true.
           perform FORM1-IUD-DISPLAY.

           perform RESETTA-GRID.

           move 0 to RigheIniziali.
           move 0 to RigaCambiata ColonnaCambiata.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = X-Y
BEGIN-TEXT
           inquire gd1, cursor-y in riga,
                        cursor-x in colonna,
                        last-row in tot-righe.
END-TEXT
END-DATA
