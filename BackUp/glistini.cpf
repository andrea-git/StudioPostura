BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd1-Ev-Msg-Begin-Entry
BEGIN-TEXT
           if mod = 0
              set event-action to event-action-fail
           else
              perform X-Y
              inquire gd1(riga, 78-col-art), cell-data OLD-col-art       

              if OLD-col-art = spaces and colonna not = 1
                 set event-action to event-action-fail
              end-if

              inquire gd1(riga, 78-col-des),  cell-data OLD-col-des       
              inquire gd1(riga, 78-col-prz1), cell-data OLD-col-prz1
              inquire gd1(riga, 78-col-prz2), cell-data OLD-col-prz2
              inquire gd1(riga, 78-col-prz3), cell-data OLD-col-prz3
              
              evaluate colonna
      
              when 78-col-art
                   continue
              when 78-col-des
                   set event-action to event-action-fail 
              when other
                   set environment "KEYSTROKE" to "DATA=44 46"    
              end-evaluate
           end-if.



END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd1-Ev-Msg-Finish-Entry
BEGIN-TEXT
           set tutto-ok to true.
           perform X-Y.
           perform VALORE-RIGA.

           evaluate colonna
           when 78-col-art
                if col-art = spaces
                   if not RigaVuota
                      display message "Cancellare la riga?"
                                title titolo
                                 type mb-yes-no
                              default mb-no
                            giving scelta
                      if scelta = mb-no
                         set errori to true
                      else
                         move ef-codice-buf    to rls-codice
                         move old-rls-articolo to rls-articolo
                         delete rlistini record 
                                invalid  continue 
                         end-delete
                         initialize rec-grid 
                                    replacing numeric data by zeroes
                                         alphanumeric data by spaces

                         move riga to store-riga
                         add 1 to riga
                         perform VALORE-RIGA
                         move rls-rec to old-rls-rec
                         move store-riga to riga
                 
                         modify gd1, record-to-delete = riga
                         modify gd1, insert-rows = 1
                      end-if
                   end-if
                else
                   move col-art to art-codice
                   perform CONTROLLA-ARTICOLO-GIA-INSERITO
                   if not trovato
                      perform CONTROLLO-RIGA
                   end-if
                end-if
                if tutto-ok
                   if rls-articolo not = old-rls-articolo
                      move riga to RigaCambiata
                      move 1    to ColonnaCambiata
                   end-if
                end-if

           when 78-col-prz1
                if col-prz1 not = old-col-prz1
                   move riga         to RigaCambiata
                   move 78-col-prz1  to ColonnaCambiata
                end-if
                perform CONTROLLO-RIGA
                modify gd1(riga, 78-col-prz1), cell-data col-prz1

           when 78-col-prz2
                if col-prz2 not = old-col-prz2
                   move riga         to RigaCambiata
                   move 78-col-prz2  to ColonnaCambiata
                end-if
                perform CONTROLLO-RIGA
                modify gd1(riga, 78-col-prz2), cell-data col-prz2

           when 78-col-prz3
                if col-prz3 not = old-col-prz3
                   move riga         to RigaCambiata
                   move 78-col-prz3  to ColonnaCambiata
                end-if
                perform CONTROLLO-RIGA
                modify gd1(riga, 78-col-prz3), cell-data col-prz3
           end-evaluate.

           if errori
              move 78-ID-gd1 to store-id
              set event-action to event-action-fail
           else
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if.
END-TEXT
END-DATA
