BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-excell-LinkTo
BEGIN-TEXT
           if nuovo
              display message "Salvare prima il documento"
                        title tit-err
                         icon 2
           else
              if tls-codice = 0
                 display message "Selezionare un listino valido"
                           title tit-err
                            icon 2
                 exit paragraph
              end-if

              display message "Esportare in Excel?"
                        title titolo
                         type mb-yes-no
                         icon 2
                       giving scelta
              if scelta = mb-yes
                 initialize wstampa
                 accept  wstampa  from environment "PATH_ST"            
                 inspect lab-nome-buf replacing trailing spaces by low-value
                 inspect wstampa  replacing trailing spaces by low-value
                 accept como-data from century-date
                 accept como-ora  from time
                 string wstampa      delimited low-value
                        "LISTINO_"   delimited size
                        lab-nome-buf delimited low-value
                        "_"          delimited size
                        como-data    delimited size
                        "_"          delimited size
                        como-ora     delimited size
                        ".csv"       delimited size
                        into wstampa
                 end-string
                 inspect lab-nome-buf replacing trailing low-value by spaces
                 open output lineseq
                 initialize line-riga
                 string "LISTINO"    delimited size
                        ";"          delimited size
                        lab-nome-buf delimited size
                        into line-riga
                 end-string
                 write line-riga
                 write line-riga from spaces
                 move tls-ini-val to como-data
                 perform DATE-TO-SCREEN
                 move como-data   to data-val
                 initialize line-riga
                 string "VALIDO PER"   delimited size
                        ";"            delimited size
                        lab-tipo-buf   delimited size
                        ";"            delimited size
                        "VALIDO DAL"   delimited size
                        ";"            delimited size
                        data-val       delimited size
                        into line-riga
                 end-string
                 write line-riga
                 write line-riga from spaces
                 initialize line-riga
                 string "ARTICOLO"     delimited size
                        ";"            delimited size
                        "DESCRIZIONE " delimited size
                        ";"            delimited size
                        "PREZZO 1"     delimited size
                        ";"            delimited size
                        "PREZZO 2"     delimited size
                        ";"            delimited size
                        "PREZZO 3"     delimited size
                        into line-riga
                 end-string
                 write line-riga

                 move low-value  to rls-rec
                 move tls-codice to rls-codice
                 start rlistini key >= rls-chiave
                       invalid continue
                   not invalid
                       perform until 1 = 2
                          read rlistini next at end exit perform end-read
                          if rls-codice not = tls-codice
                             exit perform
                          end-if
                          move rls-articolo to art-codice
                          read articoli no lock
                               invalid move "*** NON TROVATO ***" to art-descrizione
                          end-read      
                          move rls-prezzo1 to edit-prz1
                          move rls-prezzo2 to edit-prz2
                          move rls-prezzo3 to edit-prz3
                          initialize line-riga
                          string rls-articolo    delimited size
                                 ";"             delimited size
                                 art-descrizione delimited size
                                 ";"             delimited size
                                 edit-prz1       delimited size
                                 ";"             delimited size
                                 edit-prz2       delimited size
                                 ";"             delimited size
                                 edit-prz3       delimited size                                 
                                 into line-riga
                          end-string
                          write line-riga
                       end-perform
                 end-start

                 close lineseq

                 initialize path-completo-cliente
                 string "START "       delimited size
                       x"22222022"     delimited size
                        wstampa        delimited size
                       x"22"           delimited size
                       into path-completo-cliente
                 end-string
                 call "C$SYSTEM" using path-completo-cliente, 64
                                giving StatusCode
              end-if
           end-if.
END-TEXT
END-DATA

@}}

