           05 OLD-tls-dati.
               10 OLD-tls-descrizione     PIC X(50).
               10 OLD-tls-ricerca.
                  15 OLD-tls-tipo         PIC X.
                     88 OLD-tls-tipo-categoria   VALUE IS "C".
                     88 OLD-tls-tipo-individuale VALUE IS "I".
                  15 OLD-tls-codice-tipo  PIC x(6).
                  15 OLD-tls-ini-val      PIC 9(8).
               10 OLD-tls-vuoti.
                   15 FILLER           PIC 9(18).
                   15 FILLER           PIC 9(18).
                   15 FILLER           PIC 9(18).
                   15 FILLER           PIC x(500).

       01 OLD-rls-rec.
           05 OLD-rls-chiave.
               10 OLD-rls-codice      PIC 9(5).
               10 OLD-rls-articolo    PIC X(6).
           05 OLD-rlis-dati.
               10 OLD-rls-ricerca.
                   15 OLD-rls-tipo         PIC X.
                       88 OLD-rls-tipo-catcli      VALUE "C". 
                       88 OLD-rls-tipo-individuale VALUE "I". 
                   15 OLD-rls-codice-tipo  PIC X(6).
                   15 OLD-rls-ini-val      PIC 9(8).
               10 OLD-rls-prezzo1      PIC 9(6)v999.
               10 OLD-rls-prezzo2      PIC 9(6)v999.
               10 OLD-rls-prezzo3      PIC 9(6)v999.
               10 OLD-rls-vuoti.
                   15 FILLER           PIC 9(18).
                   15 FILLER           PIC 9(18).
                   15 FILLER           PIC 9(18).
                   15 FILLER           PIC X(500).
END-TEXT
END-DATA
